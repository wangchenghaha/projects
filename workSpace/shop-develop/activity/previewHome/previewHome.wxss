.fixed-guide{
  position: fixed;
  right: 0;
  bottom: 240rpx;
  z-index: 999;

}

.fixed-guide .nick-text{
  display: block;
  font-size: 30rpx;
  line-height: 30rpx;
  max-width: 120rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #1b1a20;
  transition: all 0.3s;
}
.fixed-guide .nick-text.show{
  max-width: 750rpx;
}
.version-name{
  margin-top: 15rpx;
  padding-top: 15rpx;
  font-size: 24rpx;
  color: #ccc;
  text-align: center;
  border-top: dashed 1rpx #ccc;
}
.voucher{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.7);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9999;
  transition: 0.3s;
  transform: translateY(-100%);
}
.voucher.show{
  transform: translateY(0);
}
.voucher .advertisement{
  position: relative;
  width: 546rpx;
}
.voucher .close{
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width:120rpx;
  height: 120rpx;
}
.customer{
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
}
.customer .customer-icon{

  margin-left: 20rpx;

  width: 40rpx;

  height: 40rpx;

}


.business-text{
  padding-bottom: 30rpx;
  font-size: 24rpx;
  color: #ccc;
}
.business{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
  z-index: 999999;
}


.fixed-right{
  position: fixed;
  right: 20rpx;
  bottom: 40rpx;
  flex-direction: column;
  height: 200rpx;
}
.fixed-right .fixed-item{
  width: 80rpx;
  height: 80rpx;
  background: rgba(255, 255, 255,0.8);
  border: solid 2rpx #191919;
  border-radius: 50%;
}
.fixed-right .customer .contact{
  width: 50rpx;
  height: 50rpx;
  position: relative;
}
.fixed-right .customer .contact image{
  width: 50rpx;
  height: 50rpx;
}
.fixed-right .customer button{
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -25rpx 0 0 -25rpx;
  width: 50rpx;
  height: 50rpx;
  opacity: 0;
}
.fixed-right .go-top image{
  width: 50rpx;
  transform: rotate(-90deg);
}
