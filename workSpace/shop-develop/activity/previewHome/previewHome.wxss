/* 红包雨 */
.hbyView{
  position: fixed;
  right: 0;
  top: 270rpx;
  width: 180rpx;
  height: 180rpx;
  display: flex;
  flex-direction: column;
  z-index: 999;
}
.wish-icon{
  position: fixed;
  right: 0;
  top: 380rpx;
  width: 170rpx;
  animation: wish 2s linear infinite;
}
.wish-icon image{
  display: block;
  width: 170rpx;
  height: 153rpx;
}
@keyframes wish {
  0%, 100%{
    transform: scale(0.8) rotate(-5deg);
  }
  50%{
    transform: scale(1) rotate(5deg);
  }
}
.hbyView .gameView{
