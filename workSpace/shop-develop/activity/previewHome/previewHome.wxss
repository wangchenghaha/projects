.animate{
  position: relative;
}
.animate .gif{
  position: absolute;
  left: 50%;
  top: 220rpx;
  margin-left: -317rpx;
  width: 634rpx;
  height: 750rpx;
}
.animate .gif .gif-item{
  width: 310rpx;
  height: 750rpx;
  overflow: hidden;
}
.animate .gif .left{
  animation: moveLeft 6s linear 2s infinite;
}
.animate .gif .right{
  animation: moveRight 6s linear 2s infinite;
}
.animate .gif .gif-item .gif-left, .animate .gif .gif-item .gif-right{
  width: 634rpx;
}
.animate .gif .gif-item .gif-right{
  transform: translateX(-50%);
}
.video2{
  width: 100%;
  height: 348rpx;
}
.notice {
  width: 100%;
  height: 110rpx;
  background: #ad0d0d;
  overflow: hidden;
}
.notice .notice-text{
  height: 110rpx;
  line-height: 110rpx;
  font-size: 36rpx;
  color: #fff;
  white-space: nowrap;
  transform: translateX(100rpx);
  animation: notice 20s linear infinite;
}
@keyframes notice {
  0%{
    transform: translateX(100rpx);
  }
  99%{
    transform: translateX(-300%);
  }
  100%{
    transform: translateX(100rpx);
  }
}
