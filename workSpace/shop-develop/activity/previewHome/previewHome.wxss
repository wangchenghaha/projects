/* pages/test/test.wxss */
/* 全局公共样式 */
.flex-row{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.flex-column{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.tip-title{

  text-align: center;

  height: 60rpx;

  line-height: 60rpx;

  font-size: 36rpx;

}

.order-tel{

  align-items: center;

}

.order-tel .flex-item{

  margin-top: 20rpx;

  font-size: 32rpx;

}

.order-tel .phone-icon{

  display: block;

  width: 30rpx;

  height: 30rpx;

}

.home-page{
  overflow-x: hidden;
}
.no-scroll {
  height: auto;
  overflow: hidden;
}
.fixed-top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  overflow: hidden;
  z-index: 9999;
}

.fixed-top image {
  display: block;
  width: 100%;
}
.home-page .container{
  padding-top: 110rpx;
}
.header{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
  box-sizing: border-box;
}
.main{
  position: relative;
}
.main .mask{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 999;
}
.video{
  margin-bottom: -17rpx;
}
/* 上新 */
.new{
  margin: 0 auto;
  width: 96%;
}
.new-content{
  margin-top: 20rpx;
}
.new-content-row{
  flex: 1;
  margin: 0 10rpx;
}
/* 上新结束 */

/* 分类 */
.category .category-content{
  margin-top: 20rpx;
}
.category .category-content .category-item,.category-row{
  flex: 1;
  margin: 10rpx 10rpx 0;
}
.category image{
  display: block;
  width: 100%;
}
.category-four{
  flex-wrap: wrap;
}
.category-four .three-item{
  flex: 1;
  position: relative;
}
.category-four .three-item .three-item-timer{
  position: absolute;
  bottom: 52rpx;
  left: 40rpx;
  width: 48rpx;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  font-size: 26rpx;
  color: white;

}
.two-item{
  flex: 0 1 50%;
}
.category-four .four-item{
  flex: 0 1 25%;
}
.category-four .five-item{
  flex: 0 1 20%;
}
.category-four .six-item{
  flex: 1;
}

/* 分类结束 */

/* 瀑布流 */
.flow-item{
  flex: 1;
}
/* 瀑布流结束 */
