.container {
  width: 100%;
  height: 100%;
}
.container .context {
  width: 94%;
  margin-left: 3%;
  border-bottom: 2rpx solid black;
}
.container .context .donor {
  width: 100%;
  margin-top: 30rpx;
  height: 100rpx;
  border-bottom: 2rpx lightgray dashed;
}
.container .context .donor-info {
  width: 70%;
}
.container .context .donor-info .img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 10rpx;
}
.container .context .donor-info .info {
  width: 100%;
  margin-left: 30rpx;
}
.container .context .donor-info .info .name {
  color: #000;
  font-size: 28rpx;
}
.container .context .donor-info .info .time {
  color: gray;
  font-size: 20rpx;
}
.container .context .donor .orderStatus {
  width: 130rpx;
  line-height: 45rpx;
  text-align: center;
  border: 2rpx solid red;
  color: red;
  font-size: 22rpx;
  margin-top: 20rpx;
}
.container .context .goods-info {
  position: relative;
  width: 100%;
  margin-top: 20rpx;
  display: flex;
}
.container .context .goods-info .goods-img {
  width: 200rpx;
}
.container .context .goods-info .goods-desc {
  font-size: 24rpx;
  margin-left: 20rpx;
}
.container .context .goods-info .goods-desc .goods-name {
  padding-bottom: 20rpx;
}
.container .context .goods-info .goods-desc .goods-name text {
  padding-right: 20rpx;
}
.container .context .goods-info .express {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  border-radius: 30rpx;
  background: #000;
  color: #fff;
}
.container .context .notices {
  font-size: 24rpx;
  color: red;
  margin-top: 20rpx;
  display: flex;
}
.container .context .notices-icon {
  width: 30rpx;
  height: 30rpx;
  font-size: 24rpx;
  border-radius: 50%;
  border: red 1rpx solid;
}
.container .context .orderInfo {
  width: 100%;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.container .context .orderInfo-num {
  width: 80%;
  height: 50rpx;
  color: #000;
  background: #eeeeee;
}
.container .context .orderInfo-copy {
  width: 20%;
  height: 50rpx;
  color: #fff;
  background: #a6daed;
}
.container .context .message {
  width: 100%;
  font-size: 26rpx;
  margin-top: 20rpx;
  display: flex;
}
.container .context .message-title {
  width: 20%;
}
.container .context .message-msg {
  width: 80%;
  margin-left: 10rpx;
}

