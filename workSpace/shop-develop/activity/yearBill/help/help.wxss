page {
  width: 100%;
  height: 100%;
}

.imgView {
  width: 100%;
  height: 100%;
  display: inline-block;
  background-color: #8B74B0;
  position: relative;
}

.imgView .go {
  position: absolute;
  bottom: 15%;
  left: 0;
  width: 100%;
  text-align: center;
  color: #8801AB;
  font-size: 40rpx;
  font-family: 'jxzk';
  font-weight: 0;
}

.imgView-textView {
  position: absolute;
  top: 16%;
  left: 0;
  width: 100%;
  height: 84%;
}

.imgView-textView .text {
  text-shadow: 0.7px 0.7px 1px #1C00B5,-0.7px -0.7px 1px #1C00B5;
  font-weight: normal;
  font-size: 40rpx;
  line-height: 50rpx;
  color: #F7F2FF;
  margin-bottom: 20rpx;
  font-family: 'jxzk';
}

.imgView-textView .right {
  text-align: right;
}

.sanjiao {
  position: fixed;
  top: 4.5%;
  left: 3%;
  width: 42rpx;
  height: 59rpx;
  z-index: 8;
}
