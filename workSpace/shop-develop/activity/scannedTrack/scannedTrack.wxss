@charset "UTF-8";
.wrap {
  width: 100%;
  margin: 0 auto;
}

.icon1 {
  width: 36rpx;
  height: 36rpx;
  margin: 0rpx 10rpx;
}

.collectionHeader {
  padding: 20rpx 20rpx;
  color: #000;
}

.collectionHeader .collectionNums {
  font-size: 26rpx;
  color: #999;
}

.collectionHeader .collectionNums .txt1 {
  color: #000;
  font-weight: 600;
}

.collectionHeader .collectionOption {
  font-size: 30rpx;
}

.collectionList {
  width: 100%;
  margin-bottom: 108rpx;
}

.collectionList-item {
  width: 96%;
  margin-left: 2%;
  border-top: 10rpx solid #fcfcfc;
  display: flex;
}

.collectionList-item .btn_sel {
  width: 10%;
}

.collectionList-item .btn_sel image {
  width: 36rpx;
  height: 36rpx;
}

.collectionList-item .goods {
  display: flex;
  width: 90%;
}

.collectionList-item .goods .imagePic {
  position: relative;
}

.collectionList-item .goods .imagePic image {
  position: absolute;
  width: 160rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.collectionList-item .goods .imagePic .noStock {
  position: absolute;
  bottom: 0;
  height: 50rpx;
  line-height: 50rpx;
  width: 160rpx;
  background: rgba(0, 0, 0, 0.5);
  color: white;
  font-size: 24rpx;
  text-align: center;
}

.collectionList-item .goods-info {
  color: #000;
  font-size: 26rpx;
  margin-left: 200rpx;
}

.collectionList-item .goods-info .goodsTitle {
  width: 420rpx;
  height: 80rpx;
  margin-top: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: -webkit-box;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.collectionList-item .goods-info .base-info {
  margin-top: 120rpx;
}

.collectionList-item .goods-info .base-info .price {
  color: red;
  font-size: 32rpx;
}

.collectionList-item .goods-info .base-info .time {
  color: #999;
  font-size: 24rpx;
}

.seacherFrame {
  width: 100%;
  top: 0rpx;
  position: fixed;
  width: 100%;
  background: white;
  padding-top: 15rpx;
  padding-left: 4%;
  padding-right: 5%;
  display: flex;
  align-items: center;
}

.seacherFrame .inputArea {
  width: 84%;
  border: 2rpx solid gray;
  border-radius: 6rpx;
  height: 70rpx;
}

.seacherFrame .inputArea .seacherEdit {
  width: 80%;
  height: 70rpx;
  font-size: 32rpx;
  padding-left: 16rpx;
}

.seacherFrame .inputArea .seacher-icon {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  top: 0;
  right: 150rpx;
  z-index: 99;
  overflow: hidden;
}

.seacherFrame .inputArea .seacher-icon image {
  width: 46rpx;
  height: 46rpx;
}

.seacherFrame .canncelSeacher {
  font-size: 32rpx;
  color: gray;
  margin-left: 10rpx;
}

.noGoods {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 300rpx;
}

.noGoods .list_null_btn {
  width: 50%;
  height: 90rpx;
  font-size: 34rpx;
  line-height: 90rpx;
  background: #000;
  color: #fff;
  text-align: center;
  margin-top: 30rpx;
}

.bottomLine {
  width: 80%;
  margin-left: 10%;
  margin-top: -68rpx;
}

.bottomLine .line {
  width: 30%;
  height: 2rpx;
  background: #999;
}

.bottomLine .lineText {
  width: 40%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
}

/* 删除商品 - 弹出框 */
.tishi {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999;
}

.tishi .opacity {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: #000;
  opacity: 0.5;
}

.tishi .tanchu {
  width: 572rpx;
  height: 345rpx;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
  border-radius: 10rpx;
}

.tishi .tanchu .h2 {
  width: 100%;
  height: 100rpx;
  font-size: 38rpx;
  font-weight: 600;
  line-height: 125rpx;
  color: #000;
  text-align: center;
}

.tishi .tanchu .title {
  font-size: 30rpx;
  width: 100%;
  text-align: center;
  display: block;
  font-weight: 600;
  line-height: 145rpx;
  color: #000;
}

.tishi .tanchu .bott {
  width: 100%;
  height: 100rpx;
  clear: both;
  overflow: hidden;
}

.tishi .tanchu .bott .bot {
  float: left;
  width: 50%;
  height: 100rpx;
  border-top: 1rpx solid gray;
}

.tishi .tanchu .bott .bot .true {
  float: right;
  background: #000;
  color: #fff;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 5rpx;
}

.tishi .tanchu .bott .bot .false {
  float: left;
  color: #000;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  line-height: 100rpx;
  text-align: center;
}

.footer {
  width: 100%;
  bottom: 0rpx;
  position: fixed;
}

.footer .settlementContent {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  background: white;
  border-top: 1rpx solid gray;
  align-items: center;
}

.footer .settlementContent .left {
  width: 70%;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.footer .settlementContent .left image {
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}

.footer .settlementContent .btn {
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #000;
  color: #fff;
  font-size: 28rpx;
}
