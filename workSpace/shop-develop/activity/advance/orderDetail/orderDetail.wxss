.order-detail {
  padding-bottom: 150rpx;
  font-size: 30rpx;
  background: #eee;
}
.order-detail image {
  display: block;
  width: 100%;
}
.order-detail .step {
  padding: 20rpx 0;
  background-image: linear-gradient(to right, #F88E2A, #F95822);
}
.order-detail .step-title {
  color: #fff;
  font-size: 34rpx;
}
.order-detail .step-wrapper {
  padding: 20rpx;
  margin: 20rpx auto;
  width: 500rpx;
  height: 80rpx;
  background: rgba(255, 255, 255, 0.3);
  border-radius: 40rpx;
}
.order-detail .step-wrapper .step-item {
  font-size: 30rpx;
  color: #fff;
}
.order-detail .step-wrapper .step-item .circle {
  position: relative;
  margin: 0 auto;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  border: solid 4rpx #fff;
}
.order-detail .step-wrapper .step-item .circle.active {
  background-image: radial-gradient(rgba(255, 255, 255, 0), #fff, rgba(255, 255, 255, 0));
}
.order-detail .step-wrapper .step-item .circle .active {
  width: 12rpx;
  height: 12rpx;
  background: #fff;
  border-radius: 50rpx;
}
.order-detail .step-wrapper .step-item .circle.line:after {
  content: "";
  position: absolute;
  right: -20rpx;
  top: 15rpx;
  width: 140rpx;
  height: 4rpx;
  background: #fff;
  transform: translateX(100%);
}
.order-detail .address {
  padding: 20rpx 0;
  margin-bottom: 20rpx;
  background: #fff;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
.order-detail .address-icon {
  margin: 0 20rpx;
  width: 80rpx;
}
.order-detail .address-info {
  flex: 1;
  padding-right: 20rpx;
  font-size: 30rpx;
}
.order-detail .address-info .phone {
  color: #999;
}
.order-detail .goods-img {
  margin: 0 20rpx;
  width: 200rpx;
}
.order-detail .goods .goods-item {
  padding-top: 15rpx;
}
.order-detail .goods-size {
  margin-left: 20rpx;
}
.order-detail .goods-price {
  color: #e51c23;
}
.order-detail .goods-price text {
  font-size: 40rpx;
}
.order-detail .goods-num text {
  font-size: 36rpx;
}
.order-detail .card-item {
  padding: 20rpx;
  margin-top: 20rpx;
  background: #fff;
  border-radius: 20rpx;
}
.order-detail .pre-sale .pay-item .pay-title {
  color: #e51c23;
}
.order-detail .pre-sale .pay-item .pay-title .icon {
  display: block;
  margin-right: 30rpx;
  width: 40rpx;
  height: 40rpx;
  background: #ccc;
  border-radius: 50%;
}
.order-detail .pre-sale .pay-item .pay-title .icon.active {
  border: solid 2rpx #e51c23;
  background-image: radial-gradient(#e51c23, #fff, #fff);
}
.order-detail .pre-sale .pay-item .pay-info {
  margin-left: 20rpx;
  padding: 10rpx 10rpx 20rpx 30rpx;
  border-left: solid 2rpx #ccc;
}
.order-detail .pre-sale .pay-item .pay-info-item {
  margin-left: 20rpx;
  padding: 10rpx 0;
  border-bottom: dashed 2rpx #ccc;
}
.order-detail .pre-sale .pay-item .pay-info-item.color-c {
  color: #999;
}
.order-detail .pre-sale .pay-item .pay-info-item .price {
  font-size: 36rpx;
  color: #e51c23;
}
.order-detail .pre-sale .pay-item .pay-info-item .price.color-c {
  color: #999;
}
.order-detail .pre-sale .pay-item .pay-info .end-time {
  padding-top: 15rpx;
  padding-left: 15rpx;
  font-size: 26rpx;
}
.order-detail .voucher {
  font-size: 30rpx;
  line-height: 50rpx;
}
.order-detail .voucher .num {
  padding: 3rpx 10rpx;
  border: solid 1rpx #e51c23;
  color: #e51c23;
  font-size: 26rpx;
}
.order-detail .voucher-item {
  margin-bottom: 20rpx;
  padding: 20rpx 10rpx;
  background: #fff;
  border-radius: 10rpx;
}
.order-detail .voucher-item .voucher-price {
  font-size: 36rpx;
  color: #d4020d;
  font-weight: bold;
  text-align: center;
}
.order-detail .voucher-item .voucher-price text {
  font-size: 32rpx;
}
.order-detail .voucher-item .voucher-limit {
  font-size: 26rpx;
  color: #666666;
  text-align: center;
  width: 150rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.order-detail .voucher-item .voucher-info {
  width: 300rpx;
  font-size: 24rpx;
  color: #999999;
}
.order-detail .voucher-item .voucher-info .name {
  max-width: 300rpx;
}
.order-detail .voucher-item .card-img {
  width: 300rpx;
}
.order-detail .voucher-item .user-btn {
  width: 130rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: bold;
  color: #fff;
  background: #d4020d;
  border-radius: 25rpx;
}
.order-detail .order-item {
  padding: 15rpx 0;
}
.order-detail .order .icon-copy {
  margin-left: 8rpx;
  width: 40rpx;
}
.order-detail .btn-wrapper {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background: #fff;
  border-top: solid 2rpx #999;
  overflow: hidden;
  z-index: 9999;
}
.order-detail .btn-wrapper .pay-price {
  float: left;
  padding-left: 20rpx;
  padding-top: 30rpx;
  font-size: 30rpx;
  color: #e51c23;
}
.order-detail .btn-wrapper .pay-price text {
  font-size: 40rpx;
}
.order-detail .btn-wrapper .btn {
  float: right;
  margin: 20rpx 30rpx 0 0;
  height: 80rpx;
  border-radius: 40rpx;
  overflow: hidden;
}
.order-detail .btn-wrapper .btn-item {
  width: 180rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #e51c23;
  border-left: solid 1rpx #fff;
  box-sizing: border-box;
}
.order-detail .btn-wrapper .btn-item:first-child {
  border: none;
}
.order-detail .btn-wrapper .btn-item.disabled {
  background: #B9B4B4;
}

