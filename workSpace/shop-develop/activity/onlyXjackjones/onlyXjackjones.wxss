.container {
  width: 100%;
  background: #ffce39;
  overflow-x: hidden;
}

.container .contentArea {
  width: 100%;
  position: relative;
  display: flex;
}

.container .contentArea .images {
  width: 100%;
}

.container .contentArea .buttonLeft {
  position: absolute;
  width: 200rpx;
  height: 100rpx;
  background: none;
}

.container .images {
  width: 100%;
}

.voucher {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 999;
}

.voucher .showModel {
  position: relative;
  width: 500rpx;
  height: 500rpx;
}

.voucher .showModel .background {
  position: absolute;
  width: 100%;
}

.voucher .showModel .content {
  position: absolute;
  width: 90%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 50rpx 30rpx;
  box-sizing: border-box;
  margin-left: 5%;
  margin-top: 240rpx;
  background: none;
}

.voucher .showModel .content::after {
  border: none;
}
