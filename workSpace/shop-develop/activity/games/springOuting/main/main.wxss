.bounced {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10000;
}

.bounced-firstPop {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.bounced-firstPop .background {
  position: absolute;
  width: 80%;
}

.bounced-firstPop .share {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 800rpx;
  background: none;
  width: 300rpx;
  height: 80rpx;
}

.bounced-firstPop .share::after {
  border: none;
}

.bounced-firstPop .regretBg {
  width: 80%;
  height: 550rpx;
  background: white;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bounced-firstPop .regretBg .regretlogo {
  width: 200rpx;
  height: 62rpx;
  margin: 60rpx;
}

.bounced-firstPop .regretBg .txt1 {
  font-size: 32rpx;
  margin-top: 10rpx;
}

.bounced-firstPop .regretBg .txt2 {
  font-size: 28rpx;
  margin-top: 60rpx;
  max-width: 500rpx;
  color: #8ebe06;
}

.bounced-firstPop .regretBg .btn1 {
  width: 400rpx;
  line-height: 80rpx;
  text-align: center;
  background: #faba39;
  color: white;
  font-size: 28rpx;
  margin-top: 50rpx;
  border-radius: 80rpx;
}

.bounced-firstPop .close {
  width: 70rpx;
  height: 70rpx;
  margin-top: 100rpx;
}

.musicImg {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 22%;
  z-index: 9;
  right: 6%;
  opacity: 0.5;
}

/* 旋转class */
.music-on {
  animation: music-rotate 4s linear infinite;
}

/* 旋转动画 */
@keyframes music-rotate {
  0% {
    transform: rotateZ(0deg);
  }
  100% {
    transform: rotateZ(360deg);
  }
}
