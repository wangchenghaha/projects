.voucher {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 999;
}

.voucher .showModel {
  position: relative;
  width: 92%;
}

.voucher .showModel .rankbg {
  width: 100%;
}

.voucher .showModel .tan_close {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 50rpx;
  right: 40rpx;
}

.voucher .showModel .tab {
  position: absolute;
  width: 150rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  top: 68rpx;
}

.voucher .showModel .rankArea {
  position: absolute;
  width: 70%;
  height: 900rpx;
  z-index: 9;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.voucher .showModel .rankArea .myPoint {
  font-size: 28rpx;
  color: #ebad4c;
  margin-top: 20rpx;
}

.voucher .showModel .rankArea .threeArea {
  width: 90%;
  overflow: hidden;
}

.voucher .showModel .rankArea .threeArea .userInfo {
  flex: 1;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.voucher .showModel .rankArea .threeArea .userInfo .avatarArea {
  position: relative;
  width: 110rpx;
  height: 110rpx;
}

.voucher .showModel .rankArea .threeArea .userInfo .avatarArea .avatarbg {
  position: absolute;
  width: 110rpx;
  height: 110rpx;
}

.voucher .showModel .rankArea .threeArea .userInfo .avatarArea .avatarImg {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
}

.voucher .showModel .rankArea .threeArea .userInfo .avatarArea .level {
  position: absolute;
  width: 80rpx;
  height: 30rpx;
  text-align: center;
  font-size: 24rpx;
  color: white;
  bottom: 10rpx;
}

.voucher .showModel .rankArea .threeArea .userInfo .nickName {
  font-size: 26rpx;
  width: 110rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.voucher .showModel .rankArea .threeArea .userInfo .points {
  font-size: 28rpx;
  font-weight: 550;
  color: #6ebe27;
}

.voucher .showModel .rankArea .otherArea {
  width: 100%;
