@charset "UTF-8";
@keyframes animate {
  0% {
    opacity: 1;
    transform: translateY(0rpx);
  }
  15% {
    opacity: 1;
  }
  95% {
    opacity: 1;
  }
  100% {
    opacity: 1;
    transform: translateY(5000rpx);
  }
}

@keyframes animate1 {
  0% {
    opacity: 1;
    transform: translateY(0rpx);
  }
  15% {
    opacity: 1;
  }
  95% {
    opacity: 1;
  }
  100% {
    opacity: 1;
    transform: translateY(500rpx);
  }
}

@keyframes dropAnimate {
  0% {
    opacity: 1;
    transform: translateY(0rpx);
  }
  15% {
    opacity: 1;
  }
  95% {
    opacity: 1;
  }
  100% {
    opacity: 1;
    transform: translateY(800rpx);
  }
}

@keyframes scaleAnimate {
  0% {
    transform: scale(0.2);
    /*开始为原始大小*/
  }
  100% {
    transform: scale(1);
  }
}

