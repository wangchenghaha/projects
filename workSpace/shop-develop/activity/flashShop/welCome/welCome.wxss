.wrap{
    width:100%;
    height:100%;
    background-size: 100%;
    background-repeat: no-repeat;
    /* background-position: center center; */
  }
  .wrap image{ display:block; width:100%; height:100%;}
  .daojishi{ 
    position:fixed; 
    top:10%; 
    right:40rpx;
    width:80rpx; 
    height:80rpx; 
    line-height: 24rpx; 
    background:#333; 
    border-radius: 50%; 
    text-align:center; 
    font-size:22rpx; 
    color:#fff;   
  }
  .logoView{
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);

    width: 100%;
    text-align: center;
    color: white;
  }
  .logoView .top{
    font-size: 80rpx;
    font-weight: bold;
  }
  .logoView .center{
    margin-top: 60rpx;
    font-size: 58rpx;
  }
  .logoView .bottom{
    font-size: 47rpx;
  }