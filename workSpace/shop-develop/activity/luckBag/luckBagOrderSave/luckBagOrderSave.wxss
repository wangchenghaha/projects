page {
  background: #f5f5f5;
}

.wrapper {
  width: 100%;
  background: #f5f5f5;
}

.wrapper .expressType {
  background: white;
  height: 80rpx;
  font-size: 26rpx;
}

.wrapper .expressType .onLeft {
  margin-left: 40rpx;
}

.wrapper .expressType .onRight {
  margin-right: 40rpx;
}

.wrapper .addAderss {
  background: white;
  margin-top: 16rpx;
  height: 100rpx;
  width: 96%;
  margin-left: 2%;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.wrapper .addAderss .text {
  font-size: 32rpx;
}

.wrapper .addAderss .mark {
  width: 32rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  text-align: center;
  margin-left: 10rpx;
  border: 2rpx red solid;
  border-radius: 50%;
  color: red;
}

.wrapper .address {
  background: white;
  margin-top: 16rpx;
  width: 96%;
  margin-left: 2%;
  border-radius: 10rpx;
}

.wrapper .address .onLeft {
  margin-left: 20rpx;
}

.wrapper .address .onLeft .addressDetail {
  display: flex;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.wrapper .address .onLeft .addressDetail image {
  width: 25rpx;
  height: 30rpx;
  margin-top: 50rpx;
}

.wrapper .address .onLeft .addressDetail .detailInfo {
  max-width: 600rpx;
  margin-left: 10rpx;
  font-size: 28rpx;
}

.wrapper .address .onRight {
  margin-right: 20rpx;
}

.wrapper .goodContainer {
  background: white;
  margin-top: 16rpx;
  width: 96%;
  margin-left: 2%;
  border-radius: 10rpx;
}

.wrapper .goodContainer .goodsRow {
  width: 100%;
  font-size: 32rpx;
  color: #000;
  clear: both;
  display: flex;
}

.wrapper .goodContainer .goodsRow .onLeft {
  width: 80%;
  margin-left: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.wrapper .goodContainer .goodsRow .onRight {
  width: 20%;
  margin-right: 20rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.wrapper .couponActivity {
  background: white;
  margin-top: 16rpx;
  width: 96%;
  height: 100rpx;
  margin-left: 2%;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
}

.wrapper .couponActivity .mark {
  width: 120rpx;
  line-height: 42rpx;
  color: white;
  border-radius: 40rpx;
  text-align: center;
  font-size: 28rpx;
  background: red;
  margin-left: 20rpx;
}

.wrapper .couponActivity .content {
  font-size: 32rpx;
  margin-left: 15rpx;
}

.wrapper .costInfo {
  background: white;
  margin-top: 16rpx;
  width: 96%;
  margin-left: 2%;
  border-radius: 10rpx;
}

.wrapper .costInfo-item {
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
}

.wrapper .costInfo-item .onLeft {
  margin-left: 40rpx;
}

.wrapper .costInfo-item .onRight {
  margin-right: 40rpx;
}

.wrapper .costClass {
  height: 60rpx;
  color: #555;
}

.wrapper .dividing-line-10 {
  border-top: 10rpx solid #eee;
}

.wrapper .goodsInfo {
  display: flex;
}

.wrapper .goodsInfo .goodImg {
  display: flex;
  width: 160rpx;
  justify-content: center;
  align-items: center;
}

.wrapper .goodsInfo image {
  max-width: 260rpx;
}

.wrapper .goodsInfo .baseInfo {
  margin-left: 20rpx;
}

.wrapper .tiebiaoMargin {
  margin-bottom: 30rpx;
}

.wrapper .goodsName {
  font-size: 32rpx;
  color: #000;
  max-width: 280rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wrapper .goodsStyle {
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #555;
}

.wrapper .goodsPice {
  font-size: 32rpx;
  margin-top: 80rpx;
  color: #000;
}

.wrapper .markTag {
  width: 100rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: orangered;
  text-align: center;
  border: 1rpx solid orangered;
  border-radius: 20rpx;
  margin-top: 20rpx;
}

.wrapper .coupon-dividing-line {
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
}

.wrapper .dividing-line_1rpx {
  width: 100%;
  background: #eee;
  height: 10rpx;
}

.wrapper .bottomSubmit {
  width: 100%;
  position: fixed;
  bottom: 0;
  background: white;
  border-top: 1rpx solid #ccc;
}

.wrapper .bottomSubmit .explain {
  display: flex;
  align-items: center;
  padding: 30rpx 30rpx;
}

.wrapper .bottomSubmit .explain .content {
  font-size: 24rpx;
  color: #000;
  line-height: 25rpx;
  margin-left: 10rpx;
}

.wrapper .bottomSubmit .payInfo {
  width: 100%;
  clear: both;
}

.wrapper .bottomSubmit .payInfo .onLeft {
  float: left;
  font-size: 28rpx;
  color: #555;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 30rpx;
}

.wrapper .bottomSubmit .payInfo .onRight {
  float: right;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  width: 240rpx;
  background: #000;
  color: white;
  font-size: 32rpx;
}
