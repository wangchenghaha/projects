.bouncedBg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: black;
  opacity: 0.5;
}

.bouncedView {
  position: fixed;
  top: 30%;
  left: 20%;
  width: 60%;
  height: auto;
  border-radius: 8rpx;
  background-color: white;
  animation-name: popup;
  animation-duration: 0.3s;
  animation-timing-function: linear;
  animation-delay: 0;
  animation-iteration-count: 1;
  animation-direction: normal;
}

.bouncedView .title {
  margin-top: 70rpx;
  text-align: center;
  font-size: 30rpx;
}

.bouncedView .subTitle {
  text-align: center;
  font-size: 30rpx;
  color: red;
}

.bouncedView .imgView {
  margin-top: 30rpx;
  width: 100%;
  height: 207rpx;
  position: relative;
}

.bouncedView .imgView-img {
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 197rpx;
  height: 207rpx;
}

.bouncedView .submitView {
  margin-top: 20rpx;
  margin-left: 50rpx;
  margin-right: 50rpx;
  height: 80rpx;
  border-radius: 80rpx;
  background: linear-gradient(to right, #FF8D3B, #FF004A);
  text-align: center;
  line-height: 80rpx;
  color: white;
  font-size: 24rpx;
}

@keyframes popup {
  from {
    opacity: 0;
    transform: scale(0.3, 0.3);
  }
  to {
    opacity: 1;
    transform: scale(1, 1);
  }
}
