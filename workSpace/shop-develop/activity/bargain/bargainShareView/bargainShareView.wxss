  height: 80rpx;
  border-radius: 80rpx;
  background: linear-gradient(to right, #FF8D3B, #FF004A);
  text-align: center;
  line-height: 80rpx;
  color: white;
  font-size: 24rpx;
}

@keyframes popup {
  from {
    opacity: 0;
    transform: scale(0.3, 0.3);
  }
  to {
    opacity: 1;
    transform: scale(1, 1);
  }
}

