.bouncedView {
  position: fixed;
  top: 30%;
  left: 20%;
  width: 60%;
  height: auto;
  border-radius: 8rpx;
  background-color: white;
  animation-name: popup;
  animation-duration: 0.3s;
  animation-timing-function: linear;
  animation-delay: 0;
  animation-iteration-count: 1;
  animation-direction: normal;
}

.bouncedView .title {
  margin-top: 70rpx;
  text-align: center;
  font-size: 30rpx;
}

.bouncedView .subTitle {
  text-align: center;
  font-size: 30rpx;
  color: red;
}

.bouncedView .imgView {
  margin-top: 30rpx;
  width: 100%;
  height: 207rpx;
  position: relative;
}

.bouncedView .imgView-img {
  position: absolute;
