}

.content .bargainContainer .bargainDetail .bargainInfo .t3 {
  font-size: 24rpx;
  color: #000;
  margin-top: 10rpx;
}

.content .bargainContainer .bargainDetail .bargainInfo .t4 {
  font-size: 28rpx;
  color: red;
  margin-top: 10rpx;
}

.content .bargainContainer .dargainTime {
  display: flex;
  margin-left: 50rpx;
  margin-top: 10rpx;
}

.content .bargainContainer .dargainTime .rightTime {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #000;
}

.content .bargainContainer .dargainTime .rightTime .timer {
  width: 50rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: #000;
  border-radius: 8rpx;
  color: white;
  margin-left: 8rpx;
}

.content .bargainContainer .dargainTime .bargainBtn {
  width: 160rpx;
  line-height: 60rpx;
  text-align: center;
  color: white;
  font-size: 26rpx;
  margin-left: 20rpx;
