  align-items: center;
  padding: 10rpx;
  border-bottom: #ccc 1rpx solid;
  margin-top: 10rpx;
}

.content .select-size .addressArea .Area .addressList .left {
  width: 95%;
}

.content .select-size .addressArea .Area .addressList .left .baseInfo {
  font-size: 28rpx;
  display: flex;
}

.content .select-size .addressArea .Area .addressList .left .detailInfo {
  font-size: 24rpx;
  color: gray;
}

.content .select-size .addressArea .Area .addressList .right {
  width: 5%;
}

.content .select-size .addressArea .Area .addressList .right image {
  width: 20rpx;
}

.content .select-size .addressArea .noAddress {
  width: 100%;
  margin-top: 30rpx;
  text-align: center;
  font-size: 28rpx;
  color: gray;
}

.content .select-size .addressArea .againGetAddress {
  width: 100%;
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.content .select-size .addressArea .againGetAddress .toast {
  font-size: 28rpx;
  color: gray;
}

.content .select-size .addressArea .againGetAddress .loading {
  width: 200rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  border-radius: 60rpx;
  border: 1rpx #ccc solid;
  color: gray;
  margin-top: 20rpx;
}

