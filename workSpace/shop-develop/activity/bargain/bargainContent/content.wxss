  align-items: center;
  justify-content: center;
}

.content .bargainContainer .stocklessImg image {
  width: 140rpx;
}

.content .bargainContainer .bargainMark {
  width: 200rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  color: #000;
  background: yellow;
  border-radius: 40rpx;
  text-align: center;
  margin-left: 520rpx;
  margin-top: 10rpx;
}

.content .bargainContainer .bargainMark image {
  width: 26rpx;
