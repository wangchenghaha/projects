}

.content .goods .dargainNotice .first image {
  width: 50rpx;
  max-height: 50rpx;
  margin-left: 30rpx;
}

.content .goods .dargainNotice .first .text1 {
  font-size: 32rpx;
  color: red;
  margin-left: 10rpx;
}

.content .goods .dargainNotice .first .text2 {
  font-size: 25rpx;
  color: red;
  margin-left: 20rpx;
}

.content .goods .dargainNotice .first .refresh {
  width: 60%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.content .goods .dargainNotice .first .refresh .refreshImg {
  width: 30rpx;
  height: 30rpx;
}

.content .goods .dargainNotice .first .refresh .refreshText {
  font-size: 26rpx;
  color: #FF9348;
