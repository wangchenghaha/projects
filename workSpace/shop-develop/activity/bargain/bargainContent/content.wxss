  border-radius: 50rpx;
}

.content .bargainContainer .dargainTime .overbg {
  background: gray;
}

.content .title {
  font-size: 28rpx;
  padding: 10rpx 0;
  font-weight: 550;
}

.content .dargainUser {
  width: 94%;
  border-top: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  padding: 10rpx 0;
  margin-left: 3%;
  margin-top: 5rpx;
}

.content .dargainUser .userInfo {
  width: 100%;
  display: flex;
  align-items: center;
}

.content .dargainUser .userInfo image {
  width: 10%;
  border-radius: 50%;
  margin-left: 20rpx;
}

.content .dargainUser .userInfo .name {
  width: 60%;
  font-size: 28rpx;
  color: gray;
  margin-left: 10rpx;
}

.content .dargainUser .userInfo .time {
  width: 30%;
  font-size: 28rpx;
  color: #8B8B8B;
  text-align: end;
}

.content .dargainUser .describe {
  font-size: 24rpx;
  color: #8B8B8B;
  margin-top: 10rpx;
  margin-left: 20rpx;
}

.content .mongolia {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  position: fixed;
  left: 0;
  top: 0;
}

.content .select-size {
  position: fixed;
  left: 0;
  bottom: 0;
  padding-bottom: 30rpx;
  width: 100%;
  height: 60%;
  background: #fff;
  box-sizing: border-box;
  transition: 0.3s;
  border-radius: 10rpx;
  transform: translateY(120%);
}

.content .select-size.show {
  transition: 0.3s;
  transform: translateY(0);
}

.content .select-size .close {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}

.content .select-size .margin-auto {
  margin: 0 auto;
  width: 95%;
}

.content .select-size image {
  display: block;
  width: 100%;
}

.content .select-size .goods-base {
  padding: 10rpx 0;
  border-bottom: solid 2rpx #ccc;
  font-size: 26rpx;
  line-height: 30rpx;
}

.content .select-size .goods-base .preview {
  position: relative;
  margin-top: -50rpx;
  width: 160rpx;
}

.content .select-size .goods-base .preview image {
  border-radius: 10rpx;
}

.content .select-size .goods-base .goods-info {
  margin-left: 40rpx;
}

.content .select-size .goods-base .goods-info .price {
  color: #ff580c;
}

.content .select-size .goods-base .goods-info .stock,
.content .select-size .goods-base .goods-info .size,
.content .select-size .goods-base .goods-info .sku {
  padding: 5rpx 0;
}

.content .select-size .goods-base .goods-info .stock text,
.content .select-size .goods-base .goods-info .size text,
.content .select-size .goods-base .goods-info .sku text {
  font-size: 32rpx;
}

.content .select-size .goods-base .goods-title {
  padding: 10rpx 0 0 10rpx;
}

.content .select-size .goods-base .color {
  padding: 10rpx 0;
}

.content .select-size .goods-base .color-item {
  display: inline-block;
  padding: 0 12rpx;
  margin-right: 15rpx;
  height: 60rpx;
  font-size: 26rpx;
  line-height: 60rpx;
  background: none;
  border-radius: 10rpx;
}

.content .select-size .goods-base .color-item::after {
  border: none;
}

.content .select-size .goods-base .color-item.selected {
  background: #ff580c;
  color: #fff;
}

.content .select-size .goods-base .color-item.no-stock {
  background: #f5f5f5;
  color: #cccccc;
}

.content .select-size .goods-base .goods-num text {
  display: inline-block;
  height: 52rpx;
  line-height: 52rpx;
  font-size: 40rpx;
  text-align: center;
  border: solid 2rpx #ccc;
  border-right: none;
}

.content .select-size .goods-base .goods-num text:nth-child(2) {
  font-size: 28rpx;
}

.content .select-size .goods-base .goods-num text:last-child {
  border-right: solid 2rpx #ccc;
}

.content .select-size .goodBottom {
  padding: 20rpx 0;
  font-size: 26rpx;
  line-height: 30rpx;
}

.content .select-size .addressArea {
  width: 100%;
  margin-top: 30rpx;
  margin-bottom: 100rpx;
}

.content .select-size .addressArea .addressTitel {
