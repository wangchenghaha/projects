.bg {
  width: 100%;
  height: 100%;
}

.bg .bgImageView {
  width: 100%;
  height: auto;
  position: relative;
}

.bg .bgImageView-image {
  vertical-align: middle;
  width: 100%;
}

.bg .bgImageView .topView {
  position: absolute;
  top: 110rpx;
  left: 20rpx;
  display: flex;
  flex-direction: column;
}

.bg .bgImageView .topView .rowView {
  display: flex;
  flex-direction: row;
  width: auto;
  height: auto;
}

.bg .bgImageView .topView .rowView .leftTitle {
  font-size: 28rpx;
  color: white;
}

.bg .bgImageView .topView .rowView .leftImage {
  margin-left: 10rpx;
  width: 45rpx;
  height: auto;
}

.bg .bgImageView .topView .prizeDH {
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  width: 170rpx;
  height: 40rpx;
  border-radius: 40rpx;
  border: 1rpx solid white;
}

.bg .bgImageView .topView .prizeDH .rightImage {
  width: 30rpx;
  height: 30rpx;
  position: relative;
  left: 20rpx;
  top: 50%;
  transform: translateY(-50%);
}

.bg .bgImageView .topView .prizeDH .rightTitle {
  margin-left: 25rpx;
  font-size: 23rpx;
  color: white;
  line-height: 40rpx;
}

.bg .bgImageView .yaoqing {
  font-size: 22rpx;
  font-weight: bold;
  color: #FF8038;
  text-align: center;
  width: 300rpx;
  line-height: 42rpx;
  background-color: white;
  border-radius: 42rpx;
  position: absolute;
  top: 29%;
  left: 50%;
  transform: translateX(-50%);
}

.bg .bgImageView .tiggerView {
  position: absolute;
  top: 38%;
  left: 20%;
  width: 60%;
  height: 175rpx;
}

.bg .bgImageView .tiggerView .animationView-bg0 {
  width: 115rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  overflow: hidden;
  left: 15.5rpx;
}

.bg .bgImageView .tiggerView .animationView-bg0 .animationView {
  width: 100%;
  height: 100%;
}

.bg .bgImageView .tiggerView .animationView-bg1 {
  width: 115rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  overflow: hidden;
  left: 50%;
  transform: translate(-50%, -50%);
}

.bg .bgImageView .tiggerView .animationView-bg1 .animationView {
  width: 100%;
  height: 100%;
}

.bg .bgImageView .tiggerView .animationView-bg2 {
  width: 115rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  overflow: hidden;
  right: 15.5rpx;
}

.bg .bgImageView .tiggerView .animationView-bg2 .animationView {
  width: 100%;
  height: 100%;
}

.bg .bgImageView .xiazhu {
  font-size: 26rpx;
  font-weight: bold;
  color: white;
  position: absolute;
  top: 55.5%;
  left: 0;
  line-height: 40rpx;
  text-align: center;
  width: 100%;
  margin-left: -10rpx;
}

.bg .bgImageView .choumaView {
  position: absolute;
  top: 59.5%;
  left: 20%;
  width: 60%;
  height: 80rpx;
}

.bg .bgImageView .choumaView .leftRightBtn {
  margin-top: 5rpx;
  width: 87rpx;
  height: 70rpx;
}

.bg .bgImageView .choumaView .chouma {
  line-height: 80rpx;
  width: 100%;
  font-size: 27rpx;
  font-weight: bold;
  color: white;
  text-align: center;
}

.bg .bgImageView .choujiang {
  position: absolute;
  top: 67%;
  left: 50%;
  transform: translateX(-50%);
}

.bg .bgImageView .rulesView {
  position: absolute;
  bottom: 10%;
  left: 21%;
  width: 58%;
  height: 55rpx;
}

.bg .bgImageView .rulesView .leftRightView {
  width: 150rpx;
  height: 100%;
  line-height: 55rpx;
  font-size: 25rpx;
  color: red;
  text-align: center;
  border-radius: 55rpx;
  background-image: linear-gradient(#FCFA04, #FFA02D);
}

.bg .btn-down {
  opacity: 0.8;
  transform: scale(0.85, 0.85);
}

.bg .botomView {
  width: 100%;
  height: auto;
  background-color: #FF75FC;
  padding-bottom: 50rpx;
}

.bg .botomView .djsm {
  width: 100%;
  height: auto;
}

.bg .botomView .djsm .title {
  font-size: 28rpx;
  color: white;
  line-height: 70rpx;
}

.bg .botomView .djsm-view {
  display: flex;
  flex-direction: row;
  line-height: 50rpx;
  font-size: 25rpx;
  color: white;
}

.bg .botomView .djsm-view .leftView {
  text-align: center;
  margin-right: 20rpx;
}

.bg .botomView .djsm-view .rightView {
  flex: 1;
}

.pageViews, .bouncedView {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.bouncedView-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: black;
  opacity: .5;
}

.bouncedView-view {
  width: 80%;
  height: 600rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: white;
  border-radius: 30rpx;
  text-align: center;
}

.bouncedView-view .titles {
  width: 100%;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #030303;
  margin-bottom: 20rpx;
}

.bouncedView-view .btnView {
  width: 70%;
  height: 90rpx;
  background-image: linear-gradient(to right, #FF6826, #E70000);
  font-size: 30rpx;
  color: white;
  font-weight: bold;
  line-height: 90rpx;
  border-radius: 20rpx;
  position: relative;
  top: 50rpx;
  left: 50%;
  transform: translateX(-50%);
}

.bouncedView-paihangbang {
  width: 85%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.bouncedView-paihangbang .title {
  width: 100%;
  font-size: 32rpx;
  color: white;
  line-height: 70rpx;
  text-align: center;
  position: absolute;
  top: 0%;
  left: 0%;
}

.bouncedView-paihangbang .contentView {
  position: absolute;
  top: 7.9%;
  bottom: 6.9%;
  left: 6%;
  right: 6%;
  width: auto;
  height: auto;
  border-radius: 20rpx;
  overflow: hidden;
}

.bouncedView-paihangbang .contentView .centerView-choujiang {
  width: 100%;
  height: 90rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
}

.bouncedView-paihangbang .contentView .centerView-choujiang .leftView {
  margin-left: 40rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  border-radius: 15rpx;
  background-color: #7C27AD;
  display: flex;
  flex-direction: row;
  height: 95rpx;
}

.bouncedView-paihangbang .contentView .centerView-choujiang .leftView-image {
  width: 90rpx;
}

.bouncedView-paihangbang .contentView .centerView-choujiang .rightView {
  width: 80rpx;
  height: 31rpx;
  margin-left: 50rpx;
  margin-top: 29.5rpx;
}

.bouncedView-paihangbang .contentView .bottomView-choujiang {
  margin: 5%;
  border-radius: 20rpx;
  background-color: #7C27AD;
  width: 90%;
  height: 180rpx;
}

.bouncedView-paihangbang .contentView .bottomView-choujiang .title {
  font-size: 25rpx;
  color: white;
  line-height: 40rpx;
  text-align: center;
}

.bouncedView-paihangbang .contentView .bottomView-choujiang .subTitleView {
  width: 100%;
  height: auto;
}

.bouncedView-paihangbang .contentView .bottomView-choujiang .subTitleView .kong {
  width: 100%;
  height: 40rpx;
}

.bouncedView-paihangbang .contentView .bottomView-choujiang .subTitleView .subTitles {
  margin-left: 20rpx;
  font-size: 23rpx;
  color: white;
  line-height: 40rpx;
}

.bouncedView-paihangbang .contentView .topView {
  width: 100%;
  height: 62rpx;
  display: flex;
  flex-direction: row;
}

.bouncedView-paihangbang .contentView .topView-title {
  font-size: 25rpx;
  font-weight: bold;
  color: #FFA935;
  line-height: 62rpx;
}

.bouncedView-paihangbang .contentView .centerView {
  width: 100%;
  height: 59rpx;
  display: flex;
  flex-direction: row;
}

.bouncedView-paihangbang .contentView .centerView-other {
  width: 25%;
  height: 59rpx;
  position: relative;
}

.bouncedView-paihangbang .contentView .centerView-other-sanjiao {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: -27rpx;
  left: -32rpx;
}

.bouncedView-paihangbang .contentView .centerView-other-wo {
  font-size: 23rpx;
  color: white;
  position: absolute;
  top: -5rpx;
  left: 3rpx;
}

.bouncedView-paihangbang .contentView .centerView-other-bgView {
  position: absolute;
  top: 50%;
  left: 40rpx;
  transform: translateY(-50%);
  width: 60rpx;
  height: 30rpx;
}

.bouncedView-paihangbang .contentView .centerView-other-bgView-title {
  font-size: 22rpx;
  color: white;
  line-height: 30rpx;
  margin-left: 10rpx;
}

.bouncedView-paihangbang .contentView .centerView-other-icon {
  position: absolute;
  top: 50%;
  left: 78rpx;
  transform: translateY(-50%);
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
  border: 1rpx solid white;
}

.bouncedView-paihangbang .contentView .centerView-title {
  font-size: 24rpx;
  color: #FFA935;
  line-height: 59rpx;
  overflow: hidden;
}

.bouncedView-paihangbang .contentView .myPrize {
  position: absolute;
  bottom: 40rpx;
  left: 0;
  width: 100%;
  line-height: 45rpx;
  color: white;
  font-size: 30rpx;
  text-align: center;
}

.bouncedView-paihangbang .contentView .bottomView {
  position: absolute;
  bottom: 0;
  left: 0;
  background-color: #712799;
  font-size: 21rpx;
  font-weight: bold;
  text-align: center;
  width: 100%;
  line-height: 40rpx;
  color: #59A750;
}

.bouncedBG, .bouncedBG-opaView {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.bouncedBG-opaView {
  position: absolute;
  background-color: black;
  opacity: 0.5;
}

.bouncedBG-view {
  width: 80%;
  height: 700rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: white;
  border-radius: 30rpx;
  overflow: hidden;
}

.bouncedBG-view .title {
  font-size: 32rpx;
  font-weight: bold;
  color: #3B3B3B;
  text-align: center;
  width: 100%;
  line-height: 80rpx;
}

.bouncedBG-view-close {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  font-size: 32rpx;
  width: 40rpx;
  height: 40rpx;
}

.bouncedBG-view .listView {
  margin: 4%;
  width: 92%;
  height: 150rpx;
  display: flex;
  flex-direction: row;
}

.bouncedBG-view .listView-image {
  width: 200rpx;
  height: 140rpx;
  margin-top: 5rpx;
}

.bouncedBG-view .listView .rightView {
  flex: 1;
  margin-left: 20rpx;
  width: auto;
  height: auto;
  overflow: hidden;
  position: relative;
}

.bouncedBG-view .listView .rightView .title {
  position: absolute;
  top: 5rpx;
  left: 0;
  width: auto;
  height: auto;
  font-size: 25rpx;
  line-height: 32rpx;
  color: #3B3B3B;
  text-align: left;
}

.bouncedBG-view .listView .rightView .botomTitle {
  position: absolute;
  bottom: 0;
  left: 0;
  font-size: 23rpx;
  line-height: 28rpx;
  color: #FF5761;
  text-align: left;
}

.line {
  width: 92%;
  height: 2rpx;
  background-color: #DBDBDB;
  margin-left: 4%;
}
