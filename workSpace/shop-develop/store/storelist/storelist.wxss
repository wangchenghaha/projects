.page {
    background: #eee;
  }
  .page .page-top {
    padding: 20rpx;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 32rpx;
    color: #666;
  }
  .page .page-img {
    width: 100%;
  }
  .page .page-img image {
    width: 100%;
  }
  .page .storelist {
    padding: 20rpx;
    padding-top: 0;
  }
  .page .storelist .store-item {
    padding: 20rpx;
    margin: 30rpx 0;
    background: #fff;
  }
  .page .storelist .store-item .store-address {
    margin-top: 20rpx;
    font-size: 28rpx;
    color: #999;
  }
  .page .storelist .store-item .store-bottom {
    margin-top: 20rpx;
    display: flex;
    font-size: 30rpx;
    color: #333;
  }
  .page .storelist .store-item .store-bottom .phone {
    flex: 1;
  }
  .section {
      margin-top: 30rpx;
      padding: 0 20rpx;
      box-sizing: border-box;
  }
  .section-wrap {
      padding: 20rpx;
      background: #fff;
      font-size: 28rpx;
      color: #666;
  }
  .picker {
      padding-left: 20rpx;
      font-size: 32rpx;
      color : #666;
      margin: 20rpx 0;
      box-sizing: border-box;
      width: 100%;
      height: 80rpx;
      line-height: 80rpx;
      border: 1px solid #999;
      border-radius: 5px;
  }
  