.ba-container{
  width: 100%;
  height: 100%;
  position: relative;
  background-color: #fff;
}
.ba-bg{
  margin-top: 51px;
  width: 100%;
  height: 1050rpx;
}
.nav.on{
  background-color: #009fe8;
  color: #fff;
}
.foot{
  width: 100%;
  height: 176rpx;
  position: fixed;
  left: 0;
  bottom: 0;
}
.foot-bg{
  width: 100%;
  height: 176rpx;
}
.foot .text{
  color: #fff;
  width: 300rpx;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  border: 1rpx solid #fff;
  border-radius: 32rpx;
}





