

.wrap{ width:100%; height:auto; clear:both; overflow:hidden;}
.top{ width:100%; height:auto; clear:both; overflow:hidden; position:relative;}
.top image{ display:block; width:100%; height:auto;}
.tanchu{ width:100%; height:100%; position:absolute; left:0; top:0;}
.tanchu .opacity{ width:100%; height:100%; background:#000; opacity:0.5; position:absolute; left:0; top:0;}
.user{
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  bottom:0;
  top:0;
  left:20rpx;
  margin:auto 0;
  font-size: 24rpx;
  z-index:100;
}
.user_img{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  border: 1px solid #000;
  margin-bottom: 20rpx;
  overflow:hidden;
  position:relative;
  background:#fff;
}
.user_img image{
  display:block;width:100%; position:absolute;left:0;right:0;bottom:0;top:0;margin:auto;
}

.vip_con{ width:280rpx; height:auto; position:absolute; left:250rpx; top:170rpx; font-size:28rpx; line-height:40rpx; color:#fff;}
.name{ padding-bottom:20rpx;}

.vip_card{ width:200rpx; height:auto; position:absolute; right:30rpx; bottom:60rpx; text-align:center;}
.vip_card image{ display:block; width:120rpx; height:auto; margin:0 auto;}
.vip_card .vip_name{ font-size:24rpx; line-height:40rpx; color:#fff;}
.vip_card .vip_fen{ font-size:24rpx; line-height:40rpx; color:#fff;}


.content{ width:100%; height:auto ;clear:both; overflow:hidden;margin-bottom:20rpx;}
.content .con_h2{ width:100%; height:auto ;clear:both; overflow:hidden; background:#e9e9e9;}
.content .con_h2 text{ height:96rpx; width:100%; display:block;}
.con_li{ width:50%; float:left; height:100rpx; box-sizing:border-box; padding:0 50rpx; font-size:28rpx; line-height:96rpx; text-align:center;}
.con_list text{ border-bottom:4rpx solid #0099ff;}

.con_con{ width:100%; height:auto; clear:both; overflow:hidden; padding-top:20rpx;}
.li{ width:50%; float:left; height:auto; box-sizing:border-box; padding:0 15rpx 20rpx;}
.li image{width:100%; height:556.8rpx; box-sizing:border-box; border:2rpx solid #dedede; display:flex; flex-direction:column;}
.list_h2{ font-size:26rpx; line-height:34rpx; color:#000; padding:18rpx 0 8rpx;}
.oldprice{ text-align:center; font-size:26rpx; line-height:30rpx; margin:0 auto;}
.oldprice .line{ display:inline-block; padding:0 5rpx; width:auto; height:30rpx; position:relative;}
.oldprice .border{ width:100%; height:2rpx; background:#000; position:absolute; top:15rpx;}
.nowprice{ font-size:26rpx; line-height:50rpx; color:#0099ff; text-align:center;}

.con_con2 image{ display:block; width:285rpx; height:auto; margin:0 auto;}
.goToIndex{ width:300rpx; height:100rpx; background:#000; font-size:40rpx; line-height:100rpx; color:#fff; text-align:center; margin:0 auto; border-radius:20rpx;}


.quan_brand{ width:100%; height:auto; clear:both; overflow:hidden; padding:30rpx 0 20rpx; box-sizing:border-box; }
.quan_left{ float:left; width:200rpx; height:250rpx; position:relative;}
.quan_logo{ display:block; width:160rpx; height:160rpx; position:relative; background:#000; border-radius:50%; margin:0 auto; overflow: hidden}
.quan_logo image{ display:block; width:80%; height:auto; position:absolute; left:0; right:0; bottom:0; top:0; margin:auto;}
.quan_left .text{ font-size:24rpx; line-height:46rpx; color:#333; text-align:center;}
.quan_ul{ float:left; width:530rpx; height:auto;}
.quan_ul .zanwu{ width:100%; height:160rpx; font-size:30rpx; line-height:160rpx; text-align:center; color:#000;}
.quan_li{ width:100%; height:auto; clear:both; overflow:hidden;padding-bottom:20rpx; margin-bottom:20rpx; border-bottom:2rpx solid #e7e7e7; display:flex;}
.li_left{ float:left; width:70%; height:auto;box-sizing:border-box; padding:20rpx; background:#f5f5f5; border-radius:10rpx 20rpx 20rpx 10rpx; border:1rpx solid #e7e7e7; }
.li_left .li_title{ font-size:26rpx; line-height:45rpx; color:#333; font-weight:bold;}
.li_left .li_con{ font-size:26rpx; line-height:34rpx; color:#000;}
.li_right{ float:left; width:30%; height:auto; background:#f5f5f5; border-radius:20rpx 10rpx 10rpx 20rpx; min-height:124rpx; position:relative; border:1rpx solid #e7e7e7; border-left:none; box-sizing:border-box; }
.li_right .li_click{ height:50rpx; font-size:26rpx; line-height:50rpx; color:#000; text-align:center; position:absolute; top:0; right:0;bottom:0; left:0; margin:auto;padding:20rpx 0;}



