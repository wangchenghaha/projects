
<view class="Lbs">
    <block wx:for="{{list}}">
        <view class="weui-cell weui-cell_access" data-lg="{{item.longitude}}" data-lt="{{item.latitude}}" data-name="{{item.branch_name}}" data-address="{{item.address}}" bindtap="getMap">
            <view class="weui-cell__bd">
                <view class="weui-media-box__title">{{item.branch_name}}</view>
                <view class="weui-media-box__desc">{{item.address}}</view>
            </view>
            <view class="weui-cell__ft weui-cell__ft_in-access">{{item.distinct}}km</view>
        </view>
    </block>
</view>
