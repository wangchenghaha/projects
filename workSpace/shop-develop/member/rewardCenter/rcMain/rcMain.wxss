.wrap{
  width:100%;
  height:100%;
  clear:both;
  background-color: #dfe7f2;
}

.navi{
  width: 100%;
  height: 8vh;
  background-color: #edf1f6;
  display: flex;
  flex-direction: row;
  align-items: center;
  position:fixed;
  top: 0;
  left: 0;
  z-index:666;
}

.navi text{
  width: 30%;
  height: 8vh;
  font-size: 35rpx;
  text-align: center;
  line-height: 8vh;
}

.navi .textSelected{
    background-color: #c3cdda;
}


.navi .navimg{
  width: 10%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.navi .navimg image{
    width: 40rpx;
    height: 40rpx;
  }


.hide{
    display: none;
}

.filter{
  width: 100%;
  background-color: white;
  border-bottom: 1rpx solid #eee ;
  position:fixed;
  top: 8vh;
  left: 0;
  z-index:666;
  box-sizing: border-box;
  animation: showAnimation .5s;
  transform-origin: top center;
}

@keyframes showAnimation{
  0%{
    transform: scaleY(0)
  }
  100%{
    transform: scaleY(1)
  }
}

.height_01{
  height: 100rpx;
}

.height_02{
  height: 200rpx;
}

.filter .filter_text{
  font-size: 30rpx;
  margin: 30rpx;
}

.classification{
  width: 160rpx;
  font-size: 28rpx;
  line-height: 4v;
  text-align: center;
}


.filter .filterContent{
  width: 100%;
  display: flex;
  flex-direction: row;
}

.filter .filterContent .classify{
    width:100%;
    height:auto;
    clear:both;
    overflow:hidden;
}

.filter .filterContent .classify .filterClass{
    float: left;
    height: 6vh;
    display: flex;
    align-items: center;
    justify-content: center;
}

.filter .filterContent .filterLayout{
    height: 8vh;
    display: flex;
    align-items: center;
    justify-content: center;
}

.filter .filterContent .filterLayout text{
    font-size: 28rpx;
    text-align: center;
    line-height: 4vh;
  }

  .filter .filterContent .filterClass text{
    font-size: 28rpx;
    text-align: center;
    line-height: 4vh;
  }


.filter .filterContent .itemSelected{
    color: #4faded;
    border-top: 2rpx solid #4faded;
    border-bottom: 2rpx solid #4faded;
}

.filter .filterContent .itemNoSelected{
  color: #aaa;
}

 .filter .filterContent .confirm{
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: center;
}

.filter .filterContent .confirm text{
  background: #eee;
  font-size: 30rpx;
  text-align: center;
  color: #000;
  padding: 10rpx 50rpx 10rpx 50rpx;
  margin-top: 30rpx;

}

.filter .takeUp{
  height: 50rpx;
  display: flex;
  justify-content: center;
}

.filter .takeUp image{
  width: 50rpx;
  height: 50rpx;
}

.gift{
    width: 90%;
    padding: 5%;
    margin-top: 8vh;
    background-color: white;
}

.gift text{
    font-size: 30rpx;
}

.gift .gift-item{
    width: 100%;
    height: 200rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1rpx solid #eee;
}

.gift .gift-item .imageArea{
    width: 120rpx;
    height: 120rpx;
    position: relative;
}

.gift .gift-item .imageArea .backImg{
  width: 120rpx;
  height: 120rpx;
  position: absolute;
}

.gift .gift-item .imageArea .noStock{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}

.gift .gift-item .gift-describe{
    width: 55%;
    display: flex;
    flex-direction: column;
    margin-left: 5%;
}

.gift .gift-item .gift-btn{
    font-size:  30rpx;
    background-color: #4faded;
    border-radius: 10rpx;
    color: white;
    padding: 10rpx  15rpx 10rpx 15rpx;
}

.gift .gift-item .class_bg{
  width: 78%;
  height: 120rpx;
  background-color: #eee;
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
}

.gift .gift-item .class_bg .couponImg{
  width: 120rpx;
}

.gift .gift-item .coupon-describe{
  width: 70%;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  padding-left: 10rpx;
}

.gift .gift-item .coupon-describe .typeClass{
  font-size: 25rpx;
  color: white;
  background-color: red;
  padding: 5rpx  10rpx 5rpx 10rpx;
  margin-left: 30rpx;
}

.gift .gift-item .couponBtn{
  width: 20%;
  height: 120rpx;
  font-size: 30rpx;
  color: white;
  background-color: red;
  text-align: center;
  border-radius: 20rpx;
  line-height: 120rpx;
}

 .giftSwp {
    height:100%;
    overflow: hidden;
    padding: 0vh 40rpx;
  }


  .giftSwp .idx-swiper {
    position: relative;
    height:100%;
  }

  .giftSwp  .box {
    z-index: 0;
  }

  .giftSwp .idx-swiper .box .isShow{
    width: 90%;
    height: 88%;
    position: absolute;
    top: 0;
    border: 2px solid #ffffff;
    border-radius: 5px;
    background-color: #ffffff;
    box-shadow: 1px 1px 1px 1px #cccccc;
  }

  .giftSwp .idx-swiper .box .iPhoneXHeigth{
    height: 84%;
  }

  .giftSwp .idx-swiper .box .isShow .idx-cswp {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
  }

  .box .isShow:nth-child(1) {
    transform: scale(0.7) translateX(-30%);
    z-index: 10;
  }
  .box .isShow:nth-child(2) {
    transform: scale(0.85) translateX(-10%);
    z-index: 100;
  }

  .box .isShow:nth-child(3) {
    transform: scale(1) translateX(5%);
    z-index: 333;
  }

  .box .isShow:nth-child(4) {
    transform: scale(0.85) translateX(22%);
    z-index: 100;
  }
  .box .isShow:nth-child(5) {
    transform: scale(0.7) translateX(46%);
    z-index: 10;
  }

  .giftSwp .idx-swiper .box .isShow .idx-cswp .swp-tilte {
    margin-top: 30rpx;
    width: 60%;
    height: 60rpx;
    margin-bottom: 20rpx;
    clear:both; overflow:hidden;
  }

  .giftSwp .idx-swiper .box .isShow .idx-cswp .swp-content {
    width: 98%;
    height: 90%;
  }
  .giftSwp .idx-swiper .box .isShow .idx-cswp .noStock{
    width: 120rpx;
    height: 120rpx;
    position: absolute;
    right: 10rpx;
    top: 60rpx;
  }

  .noCoupon{
    width: 100%;
    height: 100%;
    background-color: white;
  }

  .noCoupon image{
    width: 30%;
    height: 20%;
    text-align: center;
    margin-top: 40%;
    margin-left: 35%;
  }

  .transparent_bg{
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: fixed;
    z-index: 999;
    background-color:rgba(15, 15, 26, 0.7);
    padding-right:10%;
    padding-left: 10%;
  }

  .noticeShow{
    width: 80%;
    background: white;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-bottom: 40rpx;
    margin-top: 40%;
    border-radius: 5%;
  }

  .noticeShow image{
    width: 300rpx;
    height: 300rpx;
  }

 .close{
    margin-top: 50rpx;
    margin-left: 36%;
  }

  .noticeShow text{
    font-size: 32rpx;
  }

  .noticeShow .toShop{
    width: 400rpx;
    height: 80rpx;
    font-size: 36rpx;
    color: white;
    background: black;
    line-height: 80rpx;
    text-align: center;
    margin-top: 30rpx;
  }

  .noticeShow .toOther{
    width: 400rpx;
    height: 80rpx;
    font-size: 36rpx;
    color: black;
    background: #d3d3d3;
    line-height: 80rpx;
    text-align: center;
    margin-top: 30rpx;
  }



