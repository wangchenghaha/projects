.wrap {
  min-height: 100%;
  background: #eee;
  font-size: 26rpx;
}
.wrap image {
  display: block;
  width: 100%;
}
.wrap .header {
  margin-bottom: 20rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
  border-bottom: solid 1rpx #ccc;
}
.wrap .header .logo {
  width: 288rpx;
  height: 30rpx;
}
.wrap .nav {
  position: sticky;
  top: 0;
  left: 0;
  height: 60rpx;
  background: #fff;
  border-bottom: solid 1rpx #000;
  border-top: solid 1rpx #000;
  z-index: 9;
}
.wrap .nav-item {
  flex: 1;
  line-height: 60rpx;
}
.wrap .nav-item.true {
  background: #000;
  color: #fff;
}
.wrap .content {
  margin-top: 30rpx;
}
.wrap .content .refund {
  margin: 0 auto;
  width: 95%;
}
.wrap .content .refund-item {
  align-items: flex-start;
  margin-bottom: 20rpx;
  padding: 20rpx 10rpx;
  background: #fff;
  border-radius: 20rpx;
  box-sizing: border-box;
}
.wrap .content .refund-item .exchange-img {
  width: 180rpx;
  margin-right: 20rpx;
}
.wrap .content .refund-item .exchange-info {
  flex: 1;
}
.wrap .content .refund-item .exchange-info .status {
  color: #f00;
}
.wrap .content .refund-item .exchange-info .cancel {
  margin-top: 10rpx;
}
.wrap .content .refund-item .exchange-info .cancel text {
  display: inline-block;
  padding: 5rpx 25rpx;
  border-radius: 20rpx;
  background: #000;
  color: #fff;
}
.wrap .content .no-refund {
  flex-direction: column;
  padding: 100rpx 0;
  font-size: 32rpx;
}
.wrap .content .no-refund-img {
  width: 240rpx;
  height: 240rpx;
}
.wrap .content .no-refund .tip {
  color: #ccc;
}
.wrap .content .no-refund .go-back {
  margin-top: 40rpx;
  width: 95%;
  color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  background: #000;
}

