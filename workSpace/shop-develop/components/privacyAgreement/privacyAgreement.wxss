.privacy {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #989898;
  flex-direction: column;
}
.privacy .main {
  padding: 50rpx;
  width: 590rpx;
  min-height: 630rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.privacy .main .title {
  font-size: 36rpx;
}
.privacy .main .content {
  margin-top: 40rpx;
  color: #666666;
  font-size: 28rpx;
  line-height: 50rpx;
}
.privacy .main .content .clause {
  color: #c30202;
}
.privacy .main .btn {
  margin: 80rpx auto 0;
  width: 426rpx;
  font-size: 28rpx;
}
.privacy .main .btn-agree {
  margin: 10rpx auto;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #000;
}
.privacy .main .btn-cancel {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 24rpx;
  color: #999999;
}
.privacy .logo {
  margin-top: 60rpx;
}

