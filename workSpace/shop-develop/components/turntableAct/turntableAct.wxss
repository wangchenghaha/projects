
  .tb_box{
    width: 100%;
    padding-bottom: 40rpx;
    box-sizing: border-box;
  }
  .tb_backbg{
    width: 100%;
  }
  /* 中奖名单 */
  .marquee_box {
    height: 60rpx;
    width: 100%;
    overflow: hidden;
    font-size: 28rpx;
    color: #fff;
    position: absolute;
    left: 0;
    top:0;
    opacity: 0.7;
  }
  .marquee_text {
    height:60rpx;
    line-height:60rpx;
    white-space: nowrap;
    position: absolute;
    top: 0;
  }

  /* logo */
  .tb_logo{
    width: 60%;
    height: 178rpx;
    position: absolute;
    top: 28%;
    left: 0;
  }
  .brandTitle{
    width: 100%;
    overflow: hidden;
    position: absolute;
    left: 0%;
    top:200rpx;
    display: flex;
    justify-content: center;
  }

  .brandTitle image{
    width: 60%;
  }

  /* 我的积分，优惠券列表 */
  .integral{
      display: inline-flex;
      width: 90%;
      margin-left: 5%;
      position: absolute;
      top: 47%;
      left: 0;
      color: #fff;
  }
  .integralA{
    width: 47%;
    font-size: 14px;
    text-align: center;
    border-right: 1px solid #dddcdc;
    padding-right: 3%;
  }
  .integralB{
    width: 47%;
    font-size: 14px;
    text-align: center;
    padding-left: 3%;
  }
  /* 大转盘 */
   .turntableBox{
     position: absolute;
      top: 460rpx;
      left: 0;
      width: 100%;
  }
  .container {
    z-index: 1;
    padding-top: 220rpx;
    padding-bottom: 20rpx;
    box-sizing: border-box;
    height: 100%;
    background-color: #e74143;
  }
  
  .plate-wrap-box {
    position:relative;
    width:675rpx;
    height:675rpx;
    border-radius:50%;
    margin:0 auto 60rpx;
  
  }
  
  .plate-border {
    position:relative;
    z-index:1;
    width:100%;
    height:100%;
    background: hsla(28,59%,70%,.9);
    border-radius:50%;
  
  }
  
  .plate-wrap {
    position:absolute;
    left:50%;
    top:50%;
    transform:translate(-50%, -50%);
    z-index:9;
    width:95%;
    height:95%;
    border:1rpx solid #e88605;
    border-radius:50%;
    overflow:hidden;
    margin:auto;
  
  }
  
  .plate-light {
    z-index: 1;
    width: 500rpx;
    height: 500rpx;
    border: none;
  }
  
  .plate-box {
    position: absolute;
    z-index: 1;
    left: 50%;
    width: 0;
    height: 0;
    font-size: 24rpx;
    top:0rpx;
    transform-origin: 50% 320rpx;
    border-top: 320rpx solid #ffffff;
    border-left: 184rpx solid transparent;
    border-right: 184rpx solid transparent;
  }
  
 
  .text-box {
    position:absolute;
    text-align:center;
    display:inline-block;
    width:216rpx;
    word-break:break-all;
    top:-284rpx;
    transform:translate(-50%, 0);
    font-size:26rpx;
  }
  
  .plate-btn-wrap {
    position: absolute;
    z-index: 3;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    width: 118rpx;
    height: 144rpx;
    border-radius: 50%;
  }


  .picStyle{
    width:80rpx;
    height:80rpx;
    position:absolute;
    display:inline-block;
    word-break:break-all;
    top:-198rpx;
    transform:translate(-50%, 0);
  }
  .goImg{
    width: 118rpx;
    height:144rpx;
  }
  /* 弹窗 */
  .bg{
    position: fixed;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: .5;
    z-index: 5;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
  }
  .bgWhite{
    position: fixed;
    width: 100%;
    height: 100%;
    background: #fff;
    opacity: 0;
    z-index: 5;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
  }
  .tan{
    position:fixed;
    width:100%;
    height:900rpx;
    left:0;
    top:12%;
    z-index:6;
  }
  .tan_img{
    width: 100%;
    height: 100%;
  }
  .tan_close {
      width: 55rpx;
      height: 165rpx;
      position: absolute;
      right: 9%;
  }

  .tan_text{
    position: absolute;
    width: 80%;
    height: 165rpx;
    top: 450rpx;
    left: 10%;
    line-height: 165rpx;
    text-align: center;
    z-index:999;
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
  }
  /* 再来一次按钮 */
  .btn{
    width: 100%;
    margin-top: 10%;
    display: flex
  }
  .btnP{
    width:56%;
    border-radius:50rpx;
    text-align:center;
    padding:2% 0;
    color:#fff;
    margin-left:5%;
    margin-right: 5%;
    font-size:32rpx;
    background-color:#dfae84;
  }
  /* 底部文字提示 */
  .title{
      width: 90%;
      height:30rpx;
      margin-left: 5%;
      margin-top: 60rpx;
  }
  .text{
      width: 80%;
      margin-left: 12%;
      color: #fff;
      margin-bottom: 60rpx;
  }
  .textP{
      line-height:24px;
      font-size: 28rpx;
      text-indent: -4%;
  }
  .text image{
      width: 10%;
      height:18rpx;
      margin: 5% 43%;
  }