.main {
  align-items: flex-start;
  width: 100%;
  max-height: 560rpx;
  font-size: 26rpx;
  overflow: scroll;
}
.main .level-item {
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}
.main .level-item.active {
  background: #fff;
}
.main .level {
  width: 200rpx;
  height: 560rpx;
  background: #f1f1f1;
}
.main .level.true {
  flex: 0 1 100%;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background: #fff;
  white-space: nowrap;
  border-bottom: solid 1rpx #ccc;
}
.main .level.true .level-item {
  padding: 0 20rpx;
  display: inline-block;
  border-right: solid 1rpx #ccc;
}
.main .level.true .level-item:last-child {
  border: none;
}
.main .level.true .level-item.true {
  color: red;
}
.main .level-item.true {
  background: #fff;
}
.main .level-center {
  width: 200rpx;
  height: 560rpx;
  background: #fff;
  border-right: solid #ccc 1rpx;
}
.main .level-center.true {
  background: #f1f1f1;
}
.main .level-right {
  width: 300rpx;
  height: 560rpx;
  background: #fff;
}

