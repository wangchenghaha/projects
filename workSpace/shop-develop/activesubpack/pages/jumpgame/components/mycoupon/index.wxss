.mycoupon-contaniner {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 100;
    background: rgba(0, 0, 0, 0.7);
}

.button-hover {
    background: transparent;
}

.mycoupon-wrap {
    width: 635rpx;
    height: 1041rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -317.5rpx;
    margin-top: -580rpx;
    z-index: 200;
}

.mycoupon-bg {
    width: 100%;
    height: 100%;
}

.mycoupon-content {
    display: flex;
    flex-direction: column;
    width: 520rpx;
    height: 550rpx;
    position: absolute;
    top: 385rpx;
    background: transparent;
    left: 57rpx;
    z-index: 20;
}

.address-btn {
    position: absolute;
    bottom: -49rpx;
    left: 50%;
    margin-left: -179rpx;
    width: 359rpx;
    height: 98rpx;
    z-index: 10;
}

.close-btn {
    position: absolute;
    bottom: -125rpx;
    left: 50%;
    margin-left: -25rpx;
    width: 50rpx;
    height: 50rpx;
    z-index: 10;
}

.myrank {
    display: flex;
    padding: 40rpx 0;
    justify-content: center;
    font-size: 30rpx;
    color: #f99aba;
    margin-bottom: 30rpx;
}

.my-info {
    margin: 0 20rpx;
}

.coupon-list {
    position: relative;
    flex: 1;
}

.coupon-scroll {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 10;
}

.coupon-item {
    margin-bottom: 30rpx;
}

.coupon-img {
    margin: 0 auto;
    display: block;
    width: 318rpx;
    height: 132rpx;
}

.user-btn {
    margin: 0 auto;
    display: block;
    width: 318rpx;
    height: 132rpx;
    box-shadow: none;
    background: transparent;
    border: none;
    padding: 0;
}

.user-btn::after {
    display: none;
}