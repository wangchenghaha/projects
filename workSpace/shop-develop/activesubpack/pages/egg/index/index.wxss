.egg-my-award{
  position: absolute;
  right: 0;
  top: 50%;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.egg-start-num-box{
  position: absolute;
  bottom: 50rpx;
  left: 0;
  width: 100%;
}
.egg-start-num-box .egg-num{
  color: #ffffff;
  font-size: 30rpx;
  text-align: center;
}

.canvas-box{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.canvas-box .canvas-shard{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .7);
}
.canvas-box canvas{
  position: absolute;
  width: 485rpx;
  height: 795rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}