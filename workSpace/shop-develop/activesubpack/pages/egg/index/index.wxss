.egg-container {
  position: relative;
  width: 100%;
  height: 100%;
  display: block;
  /* overflow: hidden; */
}
.egg-container .egg-bg-wrapper {
  /* position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0; */
  width: 100%;
}
.egg-container .egg-bg-wrapper image {
  width: 100%;
  display: block;
}
.egg-container .egg-wrapper {
  position: absolute;
  width: 709rpx;
  height: 1132rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.egg-wrapper image{
  width: 709rpx;
  height: 1132rpx;
}
.egg-wrapper .window-area{
  position: absolute;
  width: 395rpx;
  height: 350rpx;
  /* background: rgba(0, 0, 0, .6); */
  top: 270rpx;
  left: 150rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.window-area image{
  width: 99rpx;
  height: 99rpx;
  position: absolute;
  transition: all 200ms linear;
}
.egg-wrapper .egg-btn-area{
  position: absolute;
  width: 395rpx;
  top: 644rpx;
  left: 135rpx;
  display: flex;
  justify-content: space-between;
}
.egg-wrapper .egg-btn-area image{
  width: 135rpx;
  height: 60rpx;
}
.play-btn{
  position: absolute;
  width: 395rpx;
  height: 100rpx;
  left: 50%;
  top: 730rpx;
  /* background: #000; */
  transform: translateX(-50%);
}
