page {
  height: 100%;
}
page .main {
  width: 100%;
  height: 100%;
  position: relative;
}
page .main .bg {
  width: 750rpx;
  height: 1498rpx;
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
}
page .main .alert {
  width: 712rpx;
  height: 908rpx;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  transform: translateY(-100rpx);
  display: flex;
  flex-direction: column;
  margin: auto;
}
page .main .alert .giftList {
  width: 712rpx;
  height: 10rpx;
  flex: auto;
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  background-color: red;
}
page .main .alert .giftList .content {
  width: 712rpx;
  height: 300rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}
page .main .alert .giftList .content .imgList {
  width: 650rpx;
  height: 158rpx;
  position: relative;
}
page .main .alert .giftList .content .imgList image {
  width: 100%;
  height: 100%;
}
page .main .alert .giftList .content .imgList .btn {
  position: absolute;
  width: 133rpx;
  height: 60rpx;
  right: 50rpx;
  top: 50rpx;
}
page .main .alert .alertHead,
page .main .alert .alertBottom {
  width: 712rpx;
  height: 54rpx;
  vertical-align: bottom;
  flex: none;
}
page .main .back {
  position: absolute;
  bottom: 100rpx;
  width: 156rpx;
  height: 89rpx;
  left: 290rpx;
}
