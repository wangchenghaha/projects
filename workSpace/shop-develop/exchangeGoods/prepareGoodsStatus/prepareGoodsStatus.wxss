@import "../mycss.wxss";
@import "../tmpl/shopMultiInfo/shopMultiInfo";
/* @import "../zanui.wxss"; */

.frame{width: 100%;height: auto;display: flex;flex-direction: column; clear:both; overflow:hidden; }
.bg_white{background-color: white;}



.prepare_tip_frame{width: 100%;font-size: 26rpx;padding-bottom: 20rpx;padding-top: 17rpx;}
.tip_0{color: #FF0028;padding: 20rpx;}
.tip_time{padding-right: 20rpx;display: flex;align-items: center;color: #019FE8;width: 100%;justify-content:center;}
.img_hourglass{width: 26rpx;height: 26rpx;}

.text_line_frame{ width:100%; clear:both; overflow:hidden;font-size:24rpx;border-top: 2rpx solid #E9EBED;display: flex;flex-direction: column;padding: 17rpx 0 17rpx 0;}
.padding{padding-top: 10rpx;padding-bottom: 10rpx;}
.key_left{ float:left; padding-left:20rpx; color:#000;}
.value_right{ float:right; padding-right:20rpx; color:#646464;}
.text_bold{font-weight: bold;}


.exchange_list{width: 100%;height: auto;overflow: hidden;border-top: 1px solid #E9EBED;}
.tip_1,.tip_2{width: 100%;height: 74rpx;font-weight: bold;padding-left: 20rpx;text-align: left;
    display: flex;align-items: center;font-size: 26rpx;color: gray;margin-top: 17rpx;}
.tip_2{border-bottom: 2rpx solid #E9EBED;}

/* .price_frame{padding: 20rpx;} */
.price_expected{text-align: right;font-size: 26rpx;color: #333;border-top: 1rpx solid #E9EBED;line-height: 60rpx;padding-right: 20rpx;padding-bottom: 50rpx;}


.layout_protocol,.protocal_wrapper{width: 100%; height: 87rpx;line-height: 87rpx;display: flex;flex-direction: row;
    justify-content: space-between;align-items: center; border-top: 1px solid #EBEBEB;}
.protocal_wrapper{justify-content: flex-start;padding: 0rpx;border-top:0rpx;padding-left: 17rpx;}
.protocol_detail{font-size: 26rpx;color: #333;}
.protocal_bt{ width: 37rpx;height: 37rpx;line-height: 37rpx; font-size: 27rpx;border-radius: 50rpx;
    border: 3rpx solid #333;text-align: center;margin-left: 17rpx;}
.tip_bottom{padding: 17rpx;color: red;font-size: 26rpx;}

.frame_pop{ width:100%; height:100%; position:fixed; left:0; top:0; z-index:100000;}
.frame_opacity{ width:100%; height:100%; position:absolute; left:0; top:0; background:#000; opacity:0.5;}
.frame_rule{width: 100%;position: absolute;bottom: 0;height: 625rpx;background-color: white;
display: flex;flex-direction: column;align-items: center;}
.pop_title{width: 100%;text-align: center;height: 110rpx;line-height: 110rpx;font-size: 32rpx;}
.sv_rule{width: 100%;height: 378rpx;text-align: left;}
.rule_detail{font-size: 24rpx;margin-left: 75rpx;margin-right: 75rpx;white-space:pre-line;}
.v_gap{width: 100%;height: 7rpx;background-color: #f7f7f7;}
.pop_confirm{width: 100%;height: 130rpx;line-height: 130rpx; font-size: 30rpx;color: black;text-align: center;}