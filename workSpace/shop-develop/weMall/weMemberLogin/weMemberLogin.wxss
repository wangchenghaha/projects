.we-member .go-back {
  position: fixed;
  left: 0;
  top: 50rpx;
  font-size: 30rpx;
  background: rgba(255, 255, 255, 0.3);
  padding: 5rpx 20rpx;
  border: solid 2rpx #ccc;
  border-left: none;
  border-top-right-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  z-index: 999;
}

.we-member .content {
  padding-top: 110rpx;
  margin: 0 auto;
  width: 630rpx;
}

.we-member .content .title {
  font-size: 30rpx;
}

.we-member .content .switch-btn {
  padding: 5rpx 15rpx;
  font-size: 26rpx;
  border: solid 2rpx #000;
  box-sizing: border-box;
  border-radius: 10rpx;
}

.we-member .content .input-wrapper {
  position: relative;
  margin: 30rpx 0;
  width: 100%;
  height: 65rpx;
  overflow: hidden;
}

.we-member .content .input-wrapper input {
  position: absolute;
  left: -25%;
  top: 0;
  width: 110%;
  height: 100%;
  opacity: 0;
  z-index: 99;
  line-height: 0;
  min-height: 0;
  font-size: 1rpx;
  color: transparent;
  background: none;
}

.we-member .content .input-wrapper .number-item {
  width: 60rpx;
  height: 60rpx;
  border: solid 2rpx #000;
}

.we-member .content .input-wrapper .number .confirm {
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  background-position: center;
}

.we-member .content .example {
  font-size: 30rpx;
  line-height: 40rpx;
}

.we-member .content .guide-info {
  padding-top: 50rpx;
}

.we-member .content .guide-info .login-btn {
  padding-top: 100rpx;
}

.we-member .fixed-bottom {
  margin-top: 30rpx;
  width: 100%;
}

.we-member .fixed-bottom.fixed-bt {
  position: fixed;
  left: 0;
  bottom: 100rpx;
}

.we-member .fixed-bottom .title {
  font-size: 30rpx;
}

.we-member .fixed-bottom .logo {
  margin: 20rpx auto;
  width: 345rpx;
  height: 40rpx;
}

.we-member .fixed-bottom .logo image {
  width: 100%;
  height: 40rpx;
}

.we-member .fixed .code {
  position: relative;
  padding: 30rpx;
  height: 640rpx;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
}

.we-member .fixed .code .close {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}

.we-member .fixed .code-title {
  padding-top: 40rpx;
  font-size: 30rpx;
}

.we-member .fixed .code-wrapper {
  margin: 50rpx auto;
  width: 630rpx;
  position: relative;
  overflow: hidden;
}

.we-member .fixed .code-wrapper input {
  position: absolute;
  left: -25%;
  top: 0;
  width: 110%;
  height: 60rpx;
  z-index: 100;
  opacity: 0;
  line-height: 0;
  min-height: 0;
  font-size: 1rpx;
  color: transparent;
  background: none;
}

.we-member .fixed .code-wrapper .code-arr {
  width: 100%;
}

.we-member .fixed .code-wrapper .code-arr .code-item {
  width: 60rpx;
  height: 60rpx;
  border: solid 2rpx #000;
}

.we-member .fixed .code-wrapper button {
  margin-top: 100rpx;
}

.we-member .fixed .code .desc {
  font-size: 28rpx;
}
