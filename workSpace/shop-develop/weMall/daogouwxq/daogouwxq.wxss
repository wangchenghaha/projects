  overflow: hidden;
}
.tip-swiper .swiper .swiper-item .share-content{
  /* margin-left: 60rpx; */
  width: 290rpx;
}
.tip-swiper .swiper .swiper-item .swiper-moment{
  align-items: flex-start;

}
.tip-swiper .swiper .swiper-item .header-img{
  margin: 0 10rpx;
  width: 50rpx;
  height: 50rpx;
}
.tip-swiper .swiper .swiper-item .share-content .nick-name{
  font-size: 18rpx;
