.content image {
  display: block;
  width: 100%;
}
.content .header {
  padding: 40rpx;
  height: 280rpx;
  background: #000;
  color: #fff;
  box-sizing: border-box;
}
.content .header text {
  font-size: 32rpx;
}
.content .header text.icon {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  border: solid 2rpx #fff;
  border-radius: 50%;
}
.content .header .count {
  font-size: 24rpx;
}
.content .header .count .number {
  padding-right: 30rpx;
  font-size: 60rpx;
}
.content .customer {
  padding-top: 40rpx;
  margin-top: -40rpx;
  background: #fff;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
}
.content .customer-wrapper {
  margin: 0 auto;
  width: 720rpx;
}
.content .customer-wrapper .title {
  position: relative;
  padding-left: 20rpx;
  font-size: 30rpx;
}
.content .customer-wrapper .title:before {
  content: "";
  position: absolute;
  left: 0;
  top: 10rpx;
  width: 5rpx;
  height: 25rpx;
  background: #000;
}
.content .customer-wrapper .list {
  margin-top: 30rpx;
}
.content .customer-wrapper .list-item {
  padding-bottom: 20rpx;
  margin-bottom: 30rpx;
  border-bottom: solid 1rpx #ccc;
}
.content .customer-wrapper .list-item .info .avatar {
  margin-right: 30rpx;
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  overflow: hidden;
}
.content .customer-wrapper .list-item .info .name {
  font-size: 30rpx;
  color: #2C2C2C;
}
.content .customer-wrapper .list-item .info .price {
  font-size: 24rpx;
  color: #828282;
}
.content .customer-wrapper .list-item .time {
  font-size: 24rpx;
  color: #B3B5BA;
}

