
page{ background:#1c1b20;}
.wrap{ width:100%; height:auto; clear:both; overflow:hidden; padding-top:86rpx;}
.logo{ width:100%; height:auto; clear:both; overflow:hidden;}
.logo image{ display:block; width:30%; height:auto; margin:0 auto;}

.user{ width:100%; height:auto; clear:both; overflow:hidden; padding-top:30rpx;}
.user_img{width:136rpx; height:136rpx;border-radius:50%; margin:0 auto; overflow:hidden; background:#000;}
.user_img image{display:block; width:100%; height:100%; margin:0 auto;}
.user_name{ width:100%; height:auto; font-size:30rpx; line-height:50rpx; color:#fff; text-align:center;}

.content{ width:100%; height:auto; clear:both; overflow:hidden; padding-top:20rpx;}
.li{ width:572rpx; clear:both; overflow:hidden; margin:0 auto 25rpx;}
.li_left{ float:left; width:174rpx; font-size:30rpx; line-height:56rpx; color:#fff;}
.li_right{ float:left; width:396rpx; height:56rpx; background:#4a494e; border-radius:10rpx; position:relative;}
.li_right input{ display:block; width:100%; height:56rpx; font-size:28rpx; line-height:56rpx; color:#fff; box-sizing:border-box; padding-left:14rpx;}
.li_right .chaxun_left{ width:100%;}
.li_right .chaxun{ display:block; width:100%; height:56rpx;}
.li_right .chaxun text{ display:block; width:100%; height:56rpx;font-size:30rpx; line-height:56rpx; color:#9e8762; text-align:center; font-weight:bold;}
.li_right .yanzhengma{ width:65%;}
.li_right .fasong{ float:left; width:35%; height:56rpx; position:absolute; right:0; top:0; }
.li_right .fasong text{ float:left; width:100%; border-left:1rpx solid #fff; height:28rpx; position:absolute; right:0; top:0; bottom:0; margin:auto 0; font-size:24rpx; line-height:28rpx; color:#9e8762; text-align:center;font-weight:bold;}

.denglu{ width:572rpx; height:74rpx; font-size:30rpx; line-height:74rpx; text-align:center; margin:0 auto; background:#5f5f5f; color:#fff; margin-top:72rpx; border-radius:10rpx;}
.fixed-right{
  position: fixed;
  top: 30rpx;
  left: 0;
  padding: 8rpx 25rpx;
  font-size: 30rpx;
  color: #ffffff;
  background: rgba(255,255,255,0.2);
  border-top-right-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
.li .desc{
  font-size: 30rpx;
  color: #fff;
}