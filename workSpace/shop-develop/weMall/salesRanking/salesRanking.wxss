.container {
  width: 100%;
  height: 100%;
  position: relative;
  padding-bottom: 100rpx;
}

.container .banner {
  width: 100%;
}

.container .guideInfo {
  position: relative;
}

.container .guideInfo .background {
  width: 100%;
}

.container .guideInfo .layer {
  width: 100%;
  height: 300rpx;
  position: absolute;
  background: rgba(0, 0, 0, 0.5);
  top: 0;
  left: 0;
}

.container .guideInfo .info {
  position: absolute;
  width: 100%;
  display: flex;
  height: 200rpx;
  align-items: center;
  top: 30rpx;
  left: 0;
}

.container .guideInfo .info .pic {
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  margin-left: 50rpx;
  border: 5rpx white solid;
}

.container .guideInfo .info .data {
  margin-left: 30rpx;
}

.container .guideInfo .info .data .name {
  font-size: 32rpx;
  color: white;
  font-weight: 500;
}

.container .guideInfo .info .data .store {
  font-size: 32rpx;
  color: white;
  margin-top: 10rpx;
  font-weight: 400;
}

.container .ranking {
  border-radius: 10rpx;
  margin-top: -120rpx;
  z-index: 99;
  position: relative;
}

.container .ranking .area {
  display: flex;
  color: black;
  font-size: 34rpx;
  border-radius: 25rpx 25rpx 0 0;
  overflow: hidden;
}

.container .ranking .area .sel {
  width: 100%;
  background: #fff;
  font-weight: 550;
  text-align: center;
  line-height: 120rpx;
}

.container .ranking .area .nor {
  width: 50%;
  text-align: center;
  line-height: 120rpx;
  background: #fdf5ea;
}

.container .ranking .period {
  display: flex;
  width: 96%;
  margin-left: 2%;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}

.container .ranking .period .perCommon {
  width: 200rpx;
  line-height: 65rpx;
  font-size: 28rpx;
  border-radius: 60rpx;
  margin-left: 30rpx;
  font-weight: 500;
  text-align: center;
}

.container .ranking .period .sel {
  background: #edc192;
  color: white;
  border: 3rpx #edc192 solid;
}

.container .ranking .period .nor {
  background: white;
  color: #656565;
  border: 3rpx #e7e7e7 solid;
}

.container .ranking .rankInfo {
  display: flex;
  align-items: center;
  height: 120rpx;
  background: white;
}

.container .ranking .rankInfo .icons {
  width: 80rpx;
  margin-left: 20rpx;
}

.container .ranking .rankInfo .base {
  width: 20%;
  margin-left: 10rpx;
}

.container .ranking .rankInfo .base .name {
  font-size: 28rpx;
  color: #747371;
}

.container .ranking .rankInfo .base .area {
  font-size: 24rpx;
  color: #b6b2af;
}

.container .ranking .rankInfo .data {
  width: 40%;
}

.container .ranking .rankInfo .data .numbers {
  font-size: 28rpx;
  color: #747371;
}

.container .ranking .rankInfo .data .store {
  font-size: 24rpx;
  color: #b6b2af;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container .ranking .rankInfo .salePrice {
  font-size: 30rpx;
  color: #e08558;
}

.container .ranking .reLoadArea {
  width: 100%;
  height: 500rpx;
}

.container .ranking .reLoadArea .reLoad {
  width: 160rpx;
  line-height: 60rpx;
  border: 2rpx solid #999;
  color: #999;
  text-align: center;
}

.container .ranking .backgroundColor {
  background: #fdfaf5;
}

.container .bottomArea {
  height: 300rpx;
}

.personaldata {
  position: fixed;
  width: 90%;
  margin-left: 5%;
  background: rgba(0, 0, 0, 0.8);
  display: flex;
  align-items: center;
  justify-content: center;
  height: 200rpx;
  bottom: 68rpx;
  border-radius: 200rpx;
  z-index: 99;
}

.personaldata .data {
  width: 36%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.personaldata .data .title {
  color: white;
  font-size: 36rpx;
}

.personaldata .data .numerical {
  color: #f2c898;
  font-size: 42rpx;
  font-weight: 600;
}

.personaldata .data .compare {
  color: white;
  font-size: 32rpx;
}

.personaldata .line {
  height: 100rpx;
  width: 2rpx;
  background: white;
}
