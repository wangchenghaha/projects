.list {
  align-items: flex-start;
}
.list image {
  display: block;
  width: 100%;
}
.list-item {
  flex: 0 1 32%;
  flex-direction: column;
}
.list-item .list-img {
  flex: 1;
  width: 100%;
}
.list-item .btn-more {
  margin-top: 20rpx;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  background: #000;
  color: #fff;
  border-radius: 30rpx;
}
.list-item .price {
  margin-top: 20rpx;
}
.list-item .price-origin {
  font-size: 24rpx;
  color: #ccc;
  text-decoration: line-through;
}
.list-item .price-pay {
  font-size: 28rpx;
  font-weight: bold;
}

