.new .title {
  padding: 20rpx 0;
  font-size: 24rpx;
  color: #257bfb;
}
.new .member {
  padding: 0 30rpx;
  margin-bottom: 20rpx;
  width: 100%;
  height: 200rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.new .member .info .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  border: solid 1rpx red;
}
.new .member .info .desc {
  margin-left: 40rpx;
}
.new .member .info .desc .name {
  color: #333;
  font-size: 36rpx;
}
.new .member .info .desc .date {
  font-size: 22rpx;
  color: #999;
}
.new .member .btn {
  display: inline-block;
  width: 140rpx;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 24rpx;
  background: #257bfb;
  border-radius: 30rpx;
}

