  border-radius: 8rpx;
  background: #fff;
  z-index: 9
}

.icon-msg:after {
  content: "";
  position: absolute;
  top: 16rpx;
  right: 5rpx;
  width: 0;
  height: 0;
  border-right: 8rpx solid transparent;
  border-top: 15rpx solid #fff;
  border-left: 8rpx solid transparent;
  transform: rotate(-30deg);
  transform-origin: center top;

}

.icon-msg::before {
  content: "";
  position: absolute;
  top: 18rpx;
  right: 5rpx;
  width: 0;
  height: 0;
  border-right: 8rpx solid transparent;
  border-top: 15rpx solid #ccc;
  border-left: 8rpx solid transparent;
  transform: rotate(-30deg);
  transform-origin: center top;
}

.title-wrapper {
  display: flex;
