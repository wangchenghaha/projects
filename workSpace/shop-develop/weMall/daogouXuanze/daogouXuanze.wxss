.foot {
  width: 100%;
  height: auto;
  clear: both;
  overflow: hidden;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
  position: relative;
}

.foot_box {
  min-height: 217rpx;
  margin: 0 auto;
  box-sizing: box;
  border: 1rpx solid #9b9b9b;
  overflow: hidden;
}

.box_top {
  width: 100%;
  height: 300rpx;
  position: relative;
}

.box_top.cover {
  background: url("https://cdn.bestseller.com.cn/assets/common/image/dg/daogouxuanze1.jpg") no-repeat center center;
  background-size: cover;
}

.box_top.recommend {
  background: url("https://cdn.bestseller.com.cn/assets/common/image/dg/daogouxuanze2.jpg") no-repeat center center;
  background-size: contain;
}

.box_top1 {
  width: 300rpx;
  height: 200rpx;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

.box_top1 .box_up {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #000;
  background-size: 60%;
  border-radius: 50%;
  margin: 0 auto;
}

.box_up:after {
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  content: '';
  width: 60%;
  height: 8rpx;
  background: #fff;
}

.box_up:before {
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  content: '';
  width: 60%;
  height: 8rpx;
  background: #fff;
  transform: rotate(90deg);
  transform-origin: center center;
}

.box_top1 text {
  display: block;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #000;
  padding-top: 10rpx;
}

.upImage {
  width: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  overflow: hidden;
}

.upImage image {
  display: block;
  width: 100%;
}

.box_bottom {
  width: 100%;
  min-height: 60rpx;
