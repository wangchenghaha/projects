.ziyou_con {
  width: 100%;
  min-height: 260rpx;
  clear: both;
  overflow: hidden;
  margin: 20rpx auto;
  padding-bottom: 20rpx;
}

.ziyou_li {
  position: relative;
  float: left;
  width: 24%;
  box-sizing: border-box;
  margin-right: 1%;
  min-height: 270rpx;
  padding: 4rpx;
  border: 1rpx solid #969696;
  background-size: 25%;
  margin-bottom: 5rpx;
}

.ziyou_li .icon-upload {
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -40rpx 0 0 -40rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: #f3f3f3;
}

.ziyou_li .icon-upload::before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 60%;
  height: 8rpx;
  background: #fff;
}

.ziyou_li .icon-upload::after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 60%;
  height: 8rpx;
  background: #fff;
  transform: rotate(90deg);
  transform-origin: center center;
}

.ziyou_li image {
  display: block;
  width: 100%;
  height: auto;
}

.ziyou_li .text {
  width: 100%;
  height: 20rpx;
  overflow: hidden;
  font-size: 12rpx;
  line-height: 20rpx;
}

