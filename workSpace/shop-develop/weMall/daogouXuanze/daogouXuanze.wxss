  transition: 0.5s;
}
.title-wrapper .select-btn .arrow.rotate{
  transform: rotate(180deg);
  transform-origin: center;
}
.title-wrapper .select-btn .arrow::after{
  display: block;
  position: absolute;
  right: 13rpx;
  top:0;
  content: '';
  width: 30rpx;
  height: 4rpx;
  background: #6a6a6a;
  transform: rotate(-30deg);
  transform-origin: bottom center;
}
.title-wrapper .select-btn .arrow::before{
  display: block;
  position: absolute;
  left: 13rpx;
  top:0;
  content: '';
  width: 30rpx;
  height: 4rpx;
  background: #6a6a6a;
  transform: rotate(30deg);
  transform-origin: bottom center;
}
