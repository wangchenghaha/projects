.theme .preview{
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
}
.theme .preview .img-item{
  flex: 1;
}
.theme .preview .img-item .img image{
  display: block;
  width: 100%;
}
.theme .preview .img-item .img{
  position: relative;
  width: 330rpx;
  height: 330rpx;
  overflow: hidden;
  border: solid 2rpx #000;
}
.theme .preview .img-item .img .mask{
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  background: rgba(255,255,255,0.5);
}
.theme .mask .mask-icon{
  position: relative;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50rpx;
  background: #000;
}
.theme .mask .mask-icon::after{
  position: absolute;
  left: 25rpx;
  top: 25rpx;
  content: '';
  width: 20rpx;
  height: 20rpx;
  border: solid 4rpx #fff;
  border-radius: 50%;
}
.theme .mask .mask-icon::before{
  position: absolute;
  left: 50rpx;
  top: 45rpx;
  content: '';
  display: block;
  width: 6rpx;
  height: 20rpx;
  background: #fff;
  transform: rotate(-45deg);
  transform-origin: left center;
}
.theme .mask text{
  padding-top: 20rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.theme .preview .img-item .text{
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #666;
  text-align: center;
}
.preview-theme{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  transition: 0.3s;
  opacity: 0;
  transform: translateY(-100%);
}
.preview-theme.select{
  opacity: 1;
  transform: translateY(0);
}
.preview-theme .preview-container{
  margin: 0 auto;
  width: 85%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-end;
}
.preview-theme .theme-img{
  position: relative;
  margin-top: 40rpx;
  width: 100%;
  overflow-y: scroll;
}
.preview-theme .theme-img::before{
  position: absolute;
  right: 30rpx;
  top: -40rpx;
  content: '';
  width: 2rpx;
  height: 40rpx;
  background: #fff;
}
.preview-theme .theme-img image{
  display: block;
  width: 100%;
}
.preview-theme .close{
  position: relative;
  width: 50rpx;
  height: 50rpx;
  border: solid 4rpx #ffffff;
  border-radius: 50%;
  min-height: 50rpx;
}
.preview-theme .close::after{
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -2rpx 0 0 -15rpx ;
  content: '';
  width: 30rpx;
  height: 4rpx;
  background: #fff;
  transform: rotate(45deg);
  transform-origin: center center;
}
.preview-theme .close::before{
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -2rpx 0 0 -15rpx ;
  content: '';
  width: 30rpx;
  height: 4rpx;
  background: #fff;
  transform: rotate(-45deg);
  transform-origin: center center;
}

