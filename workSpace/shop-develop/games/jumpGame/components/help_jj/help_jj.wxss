page {
  width: 100%;
  height: 100%;
}

.bg {
  width: 100%;
  height: 100%;
  position: relative;
}

.bg .logo {
  position: absolute;
  top: 5%;
  left: 50%;
  transform: translateX(-50%);
  width: 228rpx;
}

.bg .imgView {
  width: 100%;
  height: auto;
  position: relative;
}

.bg .imgView .img {
  width: 590rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.bg .imgView .icon {
  position: absolute;
  top: 55%;
  width: 150.5rpx;
  height: 150.5rpx;
  border-radius: 150rpx;
  overflow: hidden;
}

.bg .imgView .bottomView {
  position: absolute;
  bottom: 16%;
  left: 50%;
  transform: translateX(-50%);
  width: 70%;
  height: 130rpx;
}

.sanjiao {
  position: fixed;
  top: 6%;
  left: 3%;
  width: 42rpx;
  height: 59rpx;
  z-index: 999;
}

.goHome {
  position: fixed;
  top: 40%;
  right: 32rpx;
  width: 68rpx;
  height: 68rpx;
}

.zhanwei {
  position: fixed;
  z-index: 9999;
  width: 100%;
  height: 100%;
}
