page {
  width: 100%;
  height: 100%;
  background-color: black;
}

.swiper {
  width: 100%;
  height: 100%;
}

.swiper .imgView {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}

.swiper .imgView .subView {
  width: 100%;
  position: relative;
}

.swiper .imgView .subView .btn {
  position: absolute;
  bottom: 4%;
  left: 50%;
  transform: translateX(-50%);
  width: 52%;
  height: 140rpx;
}
