.bouncedBg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: black;
  opacity: 0;
}

.bounced {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 910rpx;
  transform: translateY(100%);
}

.bounced .subView {
  width: 100%;
  height: 100%;
  position: relative;
}

.bounced .subView .closedTap {
  position: absolute;
  top: 210rpx;
  right: 20rpx;
  width: 90rpx;
  height: 100rpx;
  z-index: 2;
}

.bounced .subView .renwuViews {
  position: absolute;
  bottom: 0;
  left: 40rpx;
  right: 40rpx;
  height: 550rpx;
  overflow-y: scroll;
}

.bounced .subView .renwuViews-sub {
  width: 100%;
  height: 120rpx;
  background-color: white;
  margin-bottom: 10rpx;
  border-radius: 18rpx;
  display: flex;
  overflow: hidden;
}

.bounced .subView .renwuViews-sub .left {
  width: 120rpx;
  display: flex;
  align-items: center;
  padding-left: 40rpx;
  box-sizing: border-box;
}

.bounced .subView .renwuViews-sub .center {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 20rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.bounced .subView .renwuViews-sub .center-title {
  color: red;
  font-size: 30rpx;
}

.bounced .subView .renwuViews-sub .center-subTitle {
  color: orange;
  font-size: 23rpx;
}

.bounced .subView .renwuViews-sub .right {
  width: 180rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.bounced .subView .renwuViews-sub .right .tap {
  width: 130rpx;
  height: 60rpx;
  border-radius: 8rpx;
  background-color: #FF5156;
  color: white;
  font-size: 26rpx;
  text-align: center;
  line-height: 60rpx;
  position: relative;
}

.bounced .subView .renwuViews-sub .right .tap .share {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  background: none;
}

.bounced .subView .renwuViews-sub .right .tap .share::after {
  border: none;
}

@keyframes bouncedBgAnimation {
  0% {
    opacity: 0;
  }
  100% {
    opacity: .5;
  }
}

@keyframes bouncedBgAnimation1 {
  0% {
    opacity: .5;
  }
  100% {
    opacity: 0;
  }
}

@keyframes animate {
  0% {
    transform: translateY(100%);
  }
  100% {
    transform: translateY(0);
  }
}

@keyframes animate1 {
  0% {
    transform: translateY(0);
  }
  100% {
    transform: translateY(100%);
  }
}
