.container {
  width: 100%;
  height: 100%;
  position: flex;
}

.container .petBg {
  width: 100%;
  height: 100%;
}

.container .title {
  width: 100%;
  height: 80rpx;
  z-index: 10;
  top: 5%;
  position: absolute;
  display: flex;
  align-items: center;
}

.container .title .back {
  width: 40rpx;
  margin-left: 30rpx;
}

.container .title .tit {
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  margin-right: 70rpx;
}

.container .userInfo {
  width: 300rpx;
  height: 80rpx;
  z-index: 10;
  top: 12%;
  left: 30rpx;
  position: absolute;
  border-radius: 80rpx;
  background: rgba(255, 255, 255, 0.5);
}

.container .userInfo .userAvatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
  border: 5rpx solid #fff;
}

.container .userInfo .loverIcon {
  width: 50rpx;
  margin-left: 20rpx;
}

.container .userInfo .loverNum {
  font-size: 36rpx;
  color: #ff6d7b;
  font-weight: 900;
  margin-left: 20rpx;
}

.container .addAnim {
  z-index: 10;
  top: 13%;
  left: 260rpx;
  position: absolute;
}

.container .addAnim .loverNum {
  font-size: 36rpx;
  color: #fff;
  -webkit-text-stroke: 1px #442816;
  font-weight: 900;
  margin-left: 20rpx;
}

.container .sign {
  width: 300rpx;
  z-index: 10;
  top: 20%;
  left: 30rpx;
  position: absolute;
}

.container .sign .signIcon {
  width: 200rpx;
}

.container .petTask {
  position: absolute;
  z-index: 10;
  right: 0;
  top: 11%;
  display: flex;
  flex-direction: column;
  margin-right: 30rpx;
}

.container .petTask .img {
  width: 80rpx;
  margin-top: 30rpx;
}

.container .petOpt {
  width: 92%;
  position: absolute;
  z-index: 10;
  bottom: 50rpx;
  margin-left: 4%;
}

.container .petOpt .img {
  width: 100rpx;
  margin-right: 30rpx;
}

.container .petOpt .feed {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container .petOpt .feed .feedImg {
  width: 120rpx;
}

.container .petOpt .feed .feedNum {
  font-size: 32rpx;
  color: #fff;
  -webkit-text-stroke: 1px #442816;
  font-weight: 900;
  margin-top: -20rpx;
}

.container .petArea {
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 18%;
  left: 0;
}

.container .petArea .littlePet {
  display: inline-block;
  width: 240px;
  height: 240px;
}

.container .goHome {
  width: 200rpx;
  height: 500rpx;
  position: absolute;
  bottom: 20%;
  left: 20rpx;
}

.container .optionPop {
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 50%;
  left: 0;
}

.container .optionPop .popmsg {
  display: inline-block;
  width: 300px;
  text-align: center;
  line-height: 80rpx;
  background: rgba(240, 231, 231, 0.7);
  color: #421f1f;
  font-size: 24rpx;
  border-radius: 20rpx;
}

.container .clearArea {
  width: 100%;
  position: absolute;
  bottom: 39%;
  left: 0;
}

.container .clearArea .feces {
  position: absolute;
  width: 130rpx;
  margin-left: 105rpx;
  margin-top: 230rpx;
}

.container .clearArea .littlePet {
  position: absolute;
  width: 240px;
  height: 240px;
  top: 0;
}

.container .friendHelp {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
}

.container .friendHelp .helpContent {
  width: 100%;
  position: absolute;
  bottom: 0;
  height: 600rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container .friendHelp .helpContent .helpTitle {
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  margin-top: -80rpx;
}

.container .friendHelp .helpContent .title {
  width: 92%;
  margin-top: -25rpx;
}

.container .friendHelp .helpContent .title .helps {
  font-size: 24rpx;
  color: #000;
}

.container .friendHelp .helpContent .title .close {
  width: 40rpx;
  height: 40rpx;
}

.container .friendHelp .helpContent .showFriend {
  width: 90%;
  display: flex;
  margin-top: 80rpx;
}

.container .friendHelp .helpContent .showFriend .friendItem {
  width: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container .friendHelp .helpContent .showFriend .friendItem .friendAvatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
}

.container .friendHelp .helpContent .showFriend .friendItem .friendNick {
  max-width: 100rpx;
  font-size: 28rpx;
  color: #909090;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container .friendHelp .helpContent .notice {
  font-size: 24rpx;
  font-weight: 550;
}

.container .friendHelp .helpContent .helpBtn {
  width: 80%;
  line-height: 80rpx;
  background: #F6614C;
  color: white;
  text-align: center;
  border-radius: 80rpx;
  font-size: 28rpx;
  margin-top: 40rpx;
}

.container .friendHelp .helpContent .colorRed {
  background: #F6614C;
}

.container .friendHelp .helpContent .corlorGray {
  background: #909090;
}

.container .friendHelp .helpContent .share {
  position: absolute;
  width: 80%;
  height: 160rpx;
  bottom: 60rpx;
  background: none;
}

.container .friendHelp .helpContent .share::after {
  border: none;
}

.container .task {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 999;
}

.container .task .taskContent {
  width: 100%;
