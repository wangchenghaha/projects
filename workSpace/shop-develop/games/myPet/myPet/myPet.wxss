  bottom: 0;
  height: auto;
  padding-bottom: 20rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container .task .taskContent .taskTitle {
