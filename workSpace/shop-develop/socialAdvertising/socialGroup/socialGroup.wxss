position: fixed;left: 0;right: 0;bottom: 0;bottom: 57rpx;
font-size: 30rpx;background-color: rgba(0, 0, 0, 0.7);
}
.frame_actual_inflow{width: 100%;background-color: rgba(0, 0, 0, 0.7);font-size: 27rpx;
    height: auto;
    display: flex;flex-direction: column;align-items: center;justify-content: center;
}

.bar_1,.bar_2,.bar_3{width: 100%;color: white;
    display: flex;flex-direction: row;
}
.wrapper_logo,.wrapper_shop_info{ height: 55rpx;background-color: #2C2C2E;border: solid 1rpx #2C2C2E;
    /* display: flex;flex-direction: row;align-items: center;justify-content: center; */
   }
.wrapper_shop_info{margin-left: 10rpx;margin-right: 10rpx;color: white;}
