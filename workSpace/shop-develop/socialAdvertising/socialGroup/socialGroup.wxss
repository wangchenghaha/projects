.wrapper_logo,.wrapper_shop_info,.mini_logo{ border-radius: 13rpx;}

.wrapper_logo,.wrapper_service{margin-left: auto;margin-right: auto;}

.txt_brand{margin-left: 17rpx;margin-right: 17rpx;color:#FBD700}
