/*筛选项隐藏 显示动画 start*/
@keyframes slidedown {
  from {
    transform: scaleY(0);
    transform-origin: 100% 0;
  }

  to {
    transform: scaleY(1.0);
    transform-origin: 100% 0;
  }
}

.slidedown {
  display: block;
  animation: slidedown 0.15s ease-in both;
}

@keyframes slideup {
  from {
    transform: scaleY(1.0);
    transform-origin: 100% 0;
  }

  to {
    transform: scaleY(0);
    transform-origin: 100% 0;
  }
}

.slideup {
  display: block;
  animation: slideup 0.15s ease-in both;
}
/*筛选项隐藏 显示动画 end*/



