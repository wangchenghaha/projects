}
.we-member .operation .step {
  padding-top: 20rpx;
}
.we-member .operation .step-item {
  font-size: 32rpx;
}
.we-member .operation .step-item text {
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  border: solid 2rpx #000;
  border-radius: 50%;
}
.we-member .operation .btn-wrapper {
  padding-top: 40rpx;
