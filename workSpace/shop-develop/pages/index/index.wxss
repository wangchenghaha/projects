/* 品牌背书*/

.voucher{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.7);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9999;
  transition: 0.3s;
  transform: translateY(-100%);
}
.voucher.show{
  transform: translateY(0);
}
.voucher .advertisement{
  position: relative;
  width: 546rpx;
}
.voucher .close{
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width:120rpx;
  height: 120rpx;
}
.customer{
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
}
.customer .customer-icon{
  margin-left: 20rpx;
  width: 40rpx;
  height: 40rpx;
}

.add-coupon{
  flex-direction: column;
}
.add-coupon .tip-img{
  position: relative;
  width: 72%;
}
.add-coupon .checkbox{
  margin-top: 30rpx;
  font-size: 30rpx;
  color: rgba(255, 255, 255, 0.8);
  vertical-align: middle;
}
.add-coupon .close{
  position: absolute;
  top: 5rpx;
  right: 5rpx;
  width: 70rpx;
  height: 60rpx;
  z-index: 9;
}
.add-coupon .navigator{
  position: absolute;
  transform: translateY(-100%);
  width: 100%;
  height: 85%;
  z-index: 8;
}
.contact-button{opacity: 0;}
.img_contact{position: absolute; width: 50rpx; height: 50rpx;}

.flex-wrap{
  flex-wrap: wrap;
}
.title{
  padding: 20rpx 0;
  font-size: 36rpx;
  color: #000;
}
.video2{
  width: 100%;
  height: 348rpx;
}
/* 红包雨 */
.hbyView{
  position: fixed;
  right: 0;
  top: 270rpx;
  background: rgba(255, 255, 255, 0.6);
  border-bottom-left-radius: 39px;
  border-top-left-radius: 39px;
  display: flex;
  flex-direction: column;
  z-index: 999;
  box-sizing: border-box;
}
.wish-icon{
  position: fixed;
  right: 0;
  top: 380rpx;
  width: 170rpx;
  animation: wish 2s linear infinite;
}
.wish-icon image{
  display: block;
  width: 170rpx;
  height: 153rpx;
}
@keyframes wish {
  0%, 100%{
    transform: scale(0.8) rotate(-5deg);
  }
  50%{
    transform: scale(1) rotate(5deg);
  }
}
.hbyView .gameView{
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
}
.hbyView .game-img{
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
}
.hbyView .game-item{
  width: 186rpx;
  height: 78rpx;
}
.hbyView .game-icon{
  width: 30rpx;
  height: 30rpx;
}
.hbyView .text{
  display: block;
  font-size: 20rpx;
  line-height: 25rpx;
  color: #9D53F9;
}
.hbyView .game-name{
  display: block;
  font-size: 20rpx;
  line-height: 30rpx;
  color: #121212;
}

