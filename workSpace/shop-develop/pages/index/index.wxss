/* 全局公共样式 */
.flex-column{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.home-page{
  min-height: 150%;
  overflow-x: hidden;
}
.no-scroll {
  height: auto;
  overflow: hidden;
}
.fixed-top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  overflow: hidden;
  z-index: 9999;
}

.fixed-top image {
  display: block;
  width: 100%;
}
.home-page .container{
  padding-top: 110rpx;
}
.header{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
  box-sizing: border-box;
}

.header{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
  box-sizing: border-box;
}
.search-header{
  margin-right: 105px;
  margin-left: 15px;
  flex: 1;
  padding: 0 20rpx;
  height: 30px;
  background: #f2f2f2;
  box-sizing: border-box;
  border-radius: 15px;
  overflow: hidden;
}
.search-header .search-left{
  flex:1;
}
.search-header .icon{
  width: 30rpx;
  height: 30rpx;
}
.search-header .search-input{
  display: block;
  flex:1;
  margin-left: 10rpx;
  padding-left: 10rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #666;
}
.search-wrapper{
  margin: 0 auto;
  width: 95%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.search-wrapper .help{
  width: 50rpx;
  height: 50rpx;
  margin-right: 15rpx;
}
.search-wrapper .help image{
  width:100%;
}
.search-wrapper .search-box{
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex: 1;
  padding: 5rpx;
  border:2rpx solid #bababa;
  box-sizing:border-box;

}
.search-wrapper .search-box image{
  width: 30rpx;
  height: 30rpx;
  margin: 5px;
}
.search-box .search-input{
  flex: 1;
  height:60rpx;
  font-size:24rpx;
  line-height:60rpx;
  box-sizing:border-box;
}
.search-box .scanning{
  width: 50rpx;
  height: 50rpx;
  padding-left: 10rpx;
  border-left: 2rpx solid #bababa;
}
.header-nav{
  margin: 20rpx 0;
  display: flex;
  justify-content: space-around;
}
.header-nav image{
  display: block;
  width: 50rpx;
  height: 50rpx;
}
.header-nav .nav-item{
  position: relative;
  flex-direction: column;
  flex: 1;
}
.header-nav .nav-item .cart-num{
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: 15rpx;
  width: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  font-size: 20rpx;
  background: red;
  color: #fff;
  border-radius: 50%;
}
.header-nav .nav-text{
  padding-top: 10rpx;
  display:block;
  font-size:24rpx;
  line-height:25rpx;
  color:#494949;
  text-align:center;
}

.main{
  position: relative;
}
.main .mask{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 999;
}
.video{
  margin-bottom: -17rpx;
}

