}

.close {
  width: 50rpx;
  height: 50rpx;
  float: right;
  top: 30rpx;
  right: 30rpx;
  margin: 20rpx;
}

.couponSubmit {
  width: 60%;
  margin-left: 20%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 100rpx;
  color: #ff4001;
  position: relative;
  margin-bottom: 30rpx;
  text-align: center;
  font-size: 30rpx;
  border: 2rpx solid #ff4001;
}
.graybg {
  background: gray;
}

.animate {
  transform: scale(1);
}
.hide {
  transform: scale(0);
}
.fixed-right {
  position: fixed;
  top: 450rpx;
  right: 20rpx;
  z-index: 999;
}
.fixed-right .fixed-item {
  position: relative;
  margin-bottom: 20rpx;
  width: 76rpx;
  height: 76rpx;
  background-color: #fff;
  border-radius: 50%;
  box-shadow: 2rpx 2rpx 5rpx #ccc;
  overflow: hidden;
}
.fixed-right .fixed-item .share,
.fixed-right .fixed-item .recommend-btn {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.fixed-right .fixed-item .fixed-img {
  width: 50%;
}
.fixed-right .fixed-item .fixed-img image {
  width: 100%;
}

.dots {
  margin-top: 30rpx;
}
.dots .dots-item {
  display: inline-block;
  margin: 0 8rpx;
  width: 16rpx;
  height: 16rpx;
  background: #ccc;
  border-radius: 50%;
}
.dots .dots-item.active {
  background: #000;
}
.official {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 99999;
}

/* 轮播图 和 视频*/
.swiper-content {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.seckillShow{
  position: absolute;
  width: 100%;
  height: 100rpx;
  background: linear-gradient(to right, #49f7ff, #ec4dee);
  bottom: 0;
}

.seckillShow .originalPrice{
  width: 15%;
  font-size: 28rpx;
  color: white;
  margin-left: 4%;
}
.seckillShow .originalPrice .orginal{
  text-decoration:underline;
  text-decoration:line-through;
}
.seckillShow .seckillPrice{
  width: 35%;
}
.seckillShow .seckillPrice .seckill{
  width: 240rpx;
  line-height: 60rpx;
  background: white;
  border-radius: 60rpx;
  font-size: 26rpx;
  text-align: center;
  color: red;
}

.time{
  width: 45%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.time .timeTitle{
  font-size: 24rpx;
  color: #ffff;
}

.time .timerRow{
  margin-top: 2rpx;
}

.time .timerRow .timer{
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: #000;
  border-radius: 8rpx;
  color: white;
  margin-left: 8rpx;
  font-size: 24rpx;
}
.time .timerRow .timer.day{
  width: 60rpx;
}

.video-btn {
  position: absolute;
  bottom: 60rpx;
  left: 50%;
  transform: translateX(-50%);
  height: 50rpx;
  width: 150rpx;
  padding: 0 10rpx;
  border-radius: 60rpx;
  font-size: 24rpx;
  background-color: #fff;
  box-shadow: 1px 1px 1px #888;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
video {
  width: 600rpx;
  height: 750rpx;
}
.videoIcon {
  width: 40rpx;
  height: 40rpx;
  background-image: url(https://cdn.bestseller.com.cn/assets/common/image/detailPlay.png);
  background-size: 100%;
  background-repeat: no-repeat;
}
.share-popup{
  position: fixed;
  left: 0;
  top: 0;
  flex-direction: column;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9;
}
.share-popup .main image{
  display: block;
  width: 100%;
}
.share-popup .main{
  position: relative;
  margin-top: -60rpx;
  margin-bottom: 20rpx;
  width: 65%;
  background: #fff;
  font-size: 20rpx;
}
.share-popup .main .price{
  padding-left: 20rpx;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: red;
}
.share-popup .main .goods-name{
  padding-left: 20rpx;
}
.share-popup .main .qr{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 150rpx;
}
.canvas{
  position: absolute;
  left: 800rpx;
  top: 0;
  transform: scale(0) translate(-200%);
  z-index: -1;
  opacity: 0;
}
.goods-mark{
  font-size: 24rpx;
  color: #aaa;
}
.sub-title {
  color: red;
}
.sub-check {
  color: red;
  margin-left: 6rpx;
}
.wish-entery{
  position: fixed;
  right: 0;
  top: 180rpx;
  width: 170rpx;
  animation: wish 2s linear infinite;
}
.wish-entery image{
  display: block;
  width: 170rpx;
  height: 153rpx;
}
@keyframes wish {
  0%, 100%{
    transform: scale(0.8) rotate(-5deg);
  }
  50%{
    transform: scale(1) rotate(5deg);
  }
}
.promotion{
  padding: 10rpx 0;
  width: 100%;
  border-top: solid 2rpx #ccc;
  font-size: 28rpx;
  align-items: flex-start;
  box-sizing: border-box;
}
.promotion .promotion-text{
  width: 65rpx;
  margin-right: 20rpx;
}
.promotion .promotion-list{
  flex: 1;
}
.promotion .promotion-item{
  margin-bottom: 10rpx;
  padding: 10rpx 0;
  flex:1;
  font-size: 26rpx;
  align-items: flex-start;
}
.promotion .promotion-item:last-child{
  margin-bottom: 0;
}
.promotion .promotion-type{
  display: inline-block;
  padding: 3rpx 10rpx;
  margin-right: 10rpx;
  max-width: 200rpx;
  color: red;
  font-size: 24rpx;
  background-color: #fcedeb;
}
.promotion .promotion-desc{
  font-size: 28rpx;
  flex: 1;
}
.classify .icon{
  margin-right: 30rpx;
  width: 160rpx;
  height: 32rpx;
}
.classify .classify-title{
  flex: 1;
  padding-right: 25rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}


