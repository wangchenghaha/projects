.wrapper{
    background-color: #F3F3F3;
    height: 100%;
    position: relative;
    padding-top: 88rpx;
    box-sizing: border-box;
}
.wrapper .header {
    width: 100%;
    background: rgba(51, 51, 51, 0.3);
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
}
.wrapper .header.active {
    height: 100%;
}
.wrapper .header .content {
    background: #ffffff;
    height: 88rpx;
    overflow: hidden;
    transition: all 0.5s cubic-bezier(0.165, 0.84, 1.44, 1)
}
.wrapper .header .content.active {
    height: 208rpx;
}
.wrapper .header .topper {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    align-items: center;
    padding:0 30rpx;
    box-sizing: border-box;
}
.wrapper .header .topper .tabbox {
    display: flex;
    align-items: center;
    flex: 1;
    position: relative
}
.wrapper .header .topper .tabbox .tab-item {
    flex: 1;
    font-size: 28rpx;
    line-height: 88rpx;
    text-align: center;
    height: 88rpx;
}

.wrapper .header .topper .tabbox .underline {
    position: absolute;
    bottom: 20rpx;
    left: 0;
    width: 84rpx;
    height: 0;
    border-bottom: 2rpx solid #000;
    transform: translateX(-50%);
    transition: all 0.1s ease
}

.wrapper .header .topper .searchbar {
    padding: 23rpx 30rpx 0 60rpx;
    font-size: 0;
    box-sizing: border-box;
    height: 88rpx;
}
.wrapper .header .topper .searchbar .image {
    width: 42rpx;
    height: 42rpx;
    display: inline-block
}

.wrapper .header .content .searchbox {
    width: 100%;
    height: 120rpx;
    padding: 26rpx 30rpx;
    box-sizing: border-box;
}
.wrapper .header .content .searchbox .input {
    height: 68rpx;
    width: 100%;
    padding: 0 24rpx;
    line-height: 68rpx;
    box-sizing: border-box;
    background: #f3f3f3;
    border-radius: 4rpx;
    font-size: 30rpx;
}
.wrapper .header .content .searchbox .holder_text {
    font-size: 30rpx;
    color: #9C9C9D;
    line-height: 68rpx;
    text-align: left;
}
.wrapper .main-ceontent, .wrapper .main-ceontent .scroll-view {
    width: 100%;
    height: 100%;
    font-size: 0;
    box-sizing: border-box;
}
.wrapper .main-ceontent .user-item{
    background: #ffffff;
    margin-bottom: 10rpx;
    padding: 38rpx 60rpx;
    display: flex;
    align-items: center;
}
.wrapper .main-ceontent .user-item:first-child{
    margin-top: 10rpx;
}

.wrapper .main-ceontent .user-item .left, .wrapper .main-ceontent .user-item .right {
    flex: 1;
}
.wrapper .main-ceontent .user-item .left .name{
    font-size: 30rpx;
    color: #515151;
    line-height: 1.3;
}
.wrapper .main-ceontent .user-item .left .mobile {
    display: flex;
    align-items: center;   
    margin-top: 16rpx;
}
.wrapper .main-ceontent .user-item .left .mobile .icon {
    width: 32rpx;
    height: 32rpx;
}
.wrapper .main-ceontent .user-item .left .mobile .tit {
    font-size: 28rpx;
    padding-left: 10rpx;
    color: #5D5D5D;
}
.wrapper .main-ceontent .user-item .right {
    color: #757575;
    font-size: 24rpx;
    text-align: center;
}


.wrapper .scroll-view .nodata {
    text-align: center;
    padding-top: 222rpx;
    font-size: 0;
}

.wrapper .scroll-view .nodata .content .imagebox .image{
    width: 160rpx;
    height: 160rpx;
    display: inline-block;
}
.wrapper .scroll-view .nodata .content .tips {
    font-size: 28rpx;
    margin-top: 40rpx;
    color: #757575;
}

.wrapper .detail {
    width: 100%;
    height: 100%;
    background: rgba(51, 51, 51, 0.3);
    position: absolute;
    left: 0;
    top: 0;
}
.wrapper .detail .detail-content {
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
    left: 24rpx;
    border-radius: 20rpx;
    background: #ffffff;
    overflow: hidden
}

.wrapper .detail .detail-content .detail-header {
    display: flex;
    align-items: center;
    box-sizing: border-box;
}
.wrapper .detail .detail-content .detail-header .code {
    font-size: 24rpx;
    color: #3E3E3E;
    padding-left: 22rpx;
    flex:1
}
.wrapper .detail .detail-content .detail-header .code .con {
    color: #000
}
.wrapper .detail .detail-content .detail-header .closebox {
    padding: 22rpx;
}
.wrapper .detail .detail-content .detail-header .closebox .btn{
    width: 44rpx;
    height: 44rpx;
}

.wrapper .detail .detail-content .info {
    padding: 100rpx 0 122rpx;
    border-bottom: 1rpx dashed #979797;
}
.wrapper .detail .detail-content .info .content {
    display: flex;
    align-items: center;
}
.wrapper .detail .detail-content .info .content .name, .wrapper .detail .detail-content .info .content .mobile {
    flex: 1;
}
.wrapper .detail .detail-content .info .content .name {
    font-size: 30rpx;
    color: #5D5D5D;
    font-weight: bold;
    padding-left: 60rpx;
}
.wrapper .detail .detail-content .info .content .mobile {
    font-size: 24rpx;
    color: #333333;
}

.wrapper .detail .detail-content .form .content .title{
    font-size: 24rpx;
    color: #333333;
    padding-top: 30rpx;
    padding-left: 72rpx;
    margin-bottom: 46rpx;
}
.wrapper .detail .detail-content .form .content .checkbox {
    text-align: center;
}
.wrapper .detail .detail-content .form .content .checkbox .checkbox-item {
    display: inline-block;
    color: #333333;
    padding-right: 116rpx;
}
.wrapper .detail .detail-content .form .content .checkbox .checkbox-item .iconbox, .wrapper .detail .detail-content .form .content .checkbox .checkbox-item .tit {
    display: inline-block;
    vertical-align: top;
    text-align: center;
    line-height: 1;
    font-size: 28rpx;
}
.wrapper .detail .detail-content .form .content .checkbox .checkbox-item .iconbox {
    position: relative;
    font-size: 0;
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
    overflow: hidden;
}
.wrapper .detail .detail-content .form .content .checkbox .checkbox-item .iconbox.active::after {
    content: '';
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 16rpx;
    height: 16rpx;
    background: #000000;
    border-radius: 50%;
}
.wrapper .detail .detail-content .form .content .checkbox .checkbox-item .iconbox .icon {
    width: 32rpx;
    height: 32rpx;
    font-size: 0;
}
.wrapper .detail .detail-content .form .content .inputbox {
    margin-top: 46rpx;
    margin-bottom: 36rpx;
}
.wrapper .detail .detail-content .form .content .inputbox .input{
    width: 420rpx;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
    font-size: 30rpx;
    border: 1rpx solid #D0D0D0;
    border-radius: 4rpx;
    margin: 0 auto;
}
.wrapper .detail .detail-content .form .content .inputbox .input.disabled {
    background: rgba(0,0,0,0.1);
}
.input-placeholder {
    color: #9C9C9D;
    line-height: 64rpx;
    text-align: center;
    font-size: 24rpx;
}

.wrapper .detail .detail-content .form .content .footer {
    width: 100%;
    height: 160rpx;
    background: rgba(255,255,255,0.87);
    display: flex;
    align-items: center;
    padding: 0 44rpx;
    border-top: 1rpx solid #DBDBDB;
    box-sizing: border-box;
}

.wrapper .detail .detail-content .form .content .footer .concelbtn {
    padding-right: 42rpx;
}

.wrapper .detail .detail-content .form .content .footer .concelbtn .btn {
    width: 220rpx;
    height: 82rpx;
    border: 1rpx solid #5d5d5d;
    border-radius: 6rpx;
    background: #ffffff;
    color: #333333;
    font-size: 30rpx;
}

.wrapper .detail .detail-content .form .content .footer .btnbox {
    flex: 1;
    position: relative;
}

.btnbox .btn {
    width: 100%;
    height: 82rpx;
    line-height: 82rpx;
    text-align: center;
    padding: 0;
    border-radius: 6rpx;
    background: #3E3E3E;
    color: #ffffff;
    font-size: 32rpx;
}
.btnbox .btn.disabled{
    background: #DBDBDB;
    color: #ffffff;
}
.hoverclass {
    opacity: 0.7;
}


