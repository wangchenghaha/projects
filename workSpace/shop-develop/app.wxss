/* 轮播图 */
.recommend{ width:100%; height:480rpx; margin-bottom:20rpx; position:relative;}
.swiper-container{ width:100%;position:absolute; top:0; left:0;}
.swiper-container .swiper{height: 480rpx;}
.swiper-container .swiper .img{display:block;width: 100%; height: 100%;}
.swiper-container .dots{ position: absolute;right:0;left:0;bottom:21rpx;display: flex;  justify-content: center;margin:0 auto;}
.swiper-container .dots .dot{margin: 0 9rpx;width:18rpx;height:18rpx;border-radius: 50%;transition: all .6s; font:300 18rpx/28rpx "microsoft yahei";text-align: center; border:3rpx solid #fff;}
.swiper-container .dots .dot.active{background:#fff;color:#fff;}



/* 回到顶部 */
.goTop{position:fixed; bottom:100rpx; right:30rpx; width:60rpx; height:60rpx; z-index:999;}
.goTop image{ width:60rpx; height:60rpx;}


